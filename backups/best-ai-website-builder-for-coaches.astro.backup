      <div class="bg-white border-[3px] border-gray-300 overflow-hidden">
        <table class="w-full text-xs">
          <thead class="bg-gray-900 text-white">
            <tr>
              <th class="px-3 py-2 text-left font-black">Feature</th>
              <th class="px-3 py-2 text-left font-black">Importance</th>
              <th class="px-3 py-2 text-center font-black">Durable</th>
              <th class="px-3 py-2 text-center font-black">Framer</th>
