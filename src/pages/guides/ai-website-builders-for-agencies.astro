  date: 'January 2025'
};

const agencyBuilders = [
  {
    name: 'Framer',
    score: 8.8,
    agencyScore: 9.5,
    bestFor: 'Design-forward agencies',
    price: '$15-35/mo per site',
    features: ['Gorgeous designs', 'Code export', 'Fast performance', 'Client handoff'],
    pros: ['Beautiful results', 'Code export', 'Fast builds', 'Impressive clients'],
    cons: ['Learning curve', 'No native e-commerce', 'Design skill needed']
  },
  {
    name: 'Webflow',
    score: 8.5,
    agencyScore: 9.5,
    bestFor: 'Full-service agencies',
    price: '$23-59/mo per site',
    features: ['Complete control', 'Powerful CMS', 'Code export', 'White-label'],
    pros: ['Full customization', 'Professional results', 'CMS power', 'Code export'],
    cons: ['Steep learning', 'Can get expensive', 'Not for beginners']
  },
  {
    name: 'Mixo',
    score: 8.5,
    agencyScore: 8.5,
    bestFor: 'Rapid client delivery',
    price: '$9/mo per site',
