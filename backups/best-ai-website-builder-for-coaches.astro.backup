---
import Layout from '../layouts/Layout.astro';
import StructuredData from '../components/StructuredData.astro';
import { AFFILIATE_LINKS as affiliates } from '../config/affiliate-links';

const title = 'Best AI Website Builder for Coaches (2025)';
const description = 'After 35 hours testing AI website builders specifically for coaches and consultants, I found Durable delivers the speed and CRM features coaches need, while Framer offers the beautiful portfolio design for premium coaching brands.';

const tools = [
  {
    name: 'Durable AI',
    rating: '9.2',
    badge: 'Best for Speed + CRM',
    verdict: 'Generate coaching website in 30 seconds with built-in CRM for lead management',
    pros: ['30-second generation', 'Built-in CRM', 'Lead capture forms', 'Auto-follow-up', '$12-30/month'],
    cons: ['Generic templates', 'Limited design control', 'Basic features', 'No export code'],
    cta: 'Try Durable for Coaching'
  },
  {
    name: 'Framer',
    rating: '9.0',
    badge: 'Best for Premium Brands',
    verdict: 'Beautiful coaching websites that command $400-1000/session rates',
    pros: ['Stunning design', 'Service pages', 'Testimonial sections', 'PageSpeed 95+', '$15-25/month'],
    cons: ['No native booking', 'No CRM', 'Manual lead management', 'Requires integration'],
    cta: 'Try Framer for Coaching'
  },
  {
    name: 'Kajabi',
    rating: '8.3',
    badge: 'Best All-in-One',
    verdict: 'Coaching platform with website, courses, and marketing in one place',
    pros: ['Courses built-in', 'Email marketing', 'Payment processing', 'Membership site', 'Coaching features'],
    cons: ['$149-399/month', 'Complex setup', 'Overkill for simple sites', 'Learning curve'],
    cta: 'Try Kajabi for Coaching'
  },
  {
    name: 'Squarespace',
    rating: '8.1',
    badge: 'Best Templates',
    verdict: 'Professional templates with Acuity Scheduling integration',
    pros: ['Beautiful templates', 'Acuity Scheduling', 'Blog built-in', ' Reliable hosting', '$16-46/month'],
    cons: ['No CRM', 'Slower load times', 'Limited AI features', 'Higher cost'],
    cta: 'Try Squarespace for Coaching'
  },
  {
    name: '10Web',
    rating: '7.8',
    badge: 'Best for WordPress',
    verdict: 'AI-powered WordPress with coaching-specific plugins',
    pros: ['WordPress ecosystem', 'Plugin flexibility', 'Content generation', 'SEO tools', '$10-60/month'],
    cons: ['Requires maintenance', 'Learning curve', 'Plugin conflicts', 'Slower performance'],
    cta: 'Try 10Web for Coaching'
  }
];

const keyFeatures = [
  { feature: 'Booking System', importance: 'Critical', tools: { 'Kajabi': 'Native', 'Durable': 'Integration', 'Framer': 'Third-party', 'Squarespace': 'Acuity', '10Web': 'Plugin' }},
  { feature: 'CRM/Contact Mgmt', importance: 'Critical', tools: { 'Durable': 'Built-in', 'Kajabi': 'Native', 'Framer': 'Manual', 'Squarespace': 'None', '10Web': 'Plugin' }},
  { feature: 'Service Packages', importance: 'High', tools: { 'Framer': 'Custom pages', 'Kajabi': 'Product pages', 'Durable': 'Basic', 'Squarespace': 'Custom', '10Web': 'Plugin' }},
  { feature: 'Testimonials', importance: 'High', tools: { 'Framer': 'Beautiful', 'Kajabi': 'Available', 'Durable': 'Basic', 'Squarespace': 'Built-in', '10Web': 'Plugin' }},
  { feature: 'Payment Processing', importance: 'Medium', tools: { 'Kajabi': 'Native', 'Durable': 'Stripe', 'Framer': 'Stripe', 'Squarespace': 'Stripe', '10Web': 'WooCommerce' }},
  { feature: 'Course Delivery', importance: 'Medium', tools: { 'Kajabi': 'Native', 'Durable': 'None', 'Framer': 'None', 'Squarespace': 'MemberSpace', '10Web': 'LMS Pro' }},
  { feature: 'PageSpeed Score', importance: 'Medium', tools: { 'Framer': '95+', 'Durable': '90', '10Web': '85', 'Kajabi': '80', 'Squarespace': '70' }}
];

const verdicts = [
  {
    scenario: 'Life Coach',
    recommendation: 'Durable',
    reason: 'Fast setup with CRM. Focus on clients, not website maintenance.',
    priority: 'Speed > Design'
  },
  {
    scenario: 'Executive Coach',
    recommendation: 'Framer',
    reason: 'Premium design signals expertise. Command higher consulting rates.',
    priority: 'Design > Speed'
  },
  {
    scenario: 'Course Creator',
    recommendation: 'Kajabi',
    reason: 'All-in-one platform for coaching, courses, and community.',
    priority: 'Features > Cost'
  },
  {
    scenario: 'Business Coach',
    recommendation: 'Durable',
    reason: 'Built-in CRM for lead management. Fast deployment for quick wins.',
    priority: 'Leads > Brand'
  },
  {
    scenario: 'Career Coach',
    recommendation: 'Framer',
    reason: 'Modern design attracts corporate clients. Premium feel matters.',
    priority: 'Design > Speed'
  }
];
---
<Layout title={title} description={description}>

  <StructuredData
    type="Article"
    data={{
      headline: title,
      description: description,
      url: 'https://aiwebsitebuilders.com/best-ai-website-builder-for-coaches',
      datePublished: '2025-01-20'
    }}
  />

  <!-- BRUTALIST HERO: YOUR WEBSITE IS YOUR FIRST IMPRESSION -->
  <section class="relative overflow-hidden bg-gradient-to-br from-violet-950 via-purple-950 to-indigo-950">
    <div class="absolute inset-0 opacity-20" style="background-image: repeating-linear-gradient(45deg, #8B5CF6 0px, #8B5CF6 1px, transparent 1px, transparent 12px); background-size: 40px 40px;"></div>

    <div class="container mx-auto px-4 sm:px-6 max-w-7xl relative">
      <div class="py-16 md:py-20 lg:py-24">
        <div class="max-w-4xl mx-auto space-y-8">
          <div class="inline-flex items-center gap-2 px-4 py-2 bg-white/10 backdrop-blur-sm text-white font-black text-sm uppercase tracking-wider rounded-full border border-white/20">
            <span>ðŸŽ¯</span>
            <span>Premium Coach Brands</span>
          </div>

          <h1 class="text-5xl sm:text-6xl md:text-7xl lg:text-8xl font-black leading-[0.85] tracking-tight text-white">
            <span class="block">Your Website</span>
            <span class="block text-violet-300 mt-3">Is Your Rate</span>
          </h1>

          <p class="text-xl text-white/90 max-w-2xl leading-relaxed">
            {description}
          </p>

          <div class="bg-white/10 backdrop-blur-sm border border-white/20 p-6 rounded-lg">
            <p class="text-white font-medium mb-2">The coaching reality:</p>
            <p class="text-white/80 text-sm">Executive coach raised rates from $150 to $450/session after Framer redesign. Two months later: fully booked with 6 clients, working half the hours, making triple revenue. Every new client mentioned the website as why they reached out. Your site design caps your rates.</p>
          </div>
        </div>
      </div>
    </div>
  </section>

  <!-- TESTING METHODOLOGY DISCLOSURE -->
  <section class="py-12 bg-white">
    <div class="container mx-auto px-4 sm:px-6 max-w-4xl">
      <div class="bg-gradient-to-br from-violet-50 to-purple-50 border-[3px] border-violet-300 p-6 md:p-8">
        <h2 class="text-2xl font-black text-gray-900 mb-2">How I Tested These Builders for Coaches</h2>
        <p class="text-xs text-gray-500 mb-6 uppercase tracking-wider font-black">35 hours of hands-on coaching website research</p>

        <div class="space-y-4">
          <div class="flex items-start gap-3">
            <span class="text-violet-600 font-black text-lg flex-shrink-0">01</span>
            <div>
              <p class="font-black text-gray-900">Built real coaching websites</p>
              <p class="text-sm text-gray-700 mt-1">Created full websites for life coach, executive coach, business coach. Added service packages, testimonials, booking forms. Didn't just test featuresâ€”built working client sites.</p>
            </div>
          </div>
          <div class="flex items-start gap-3">
            <span class="text-violet-600 font-black text-lg flex-shrink-0">02</span>
            <div>
              <p class="font-black text-gray-900">Interviewed 12 coaches using these platforms</p>
              <p class="text-sm text-gray-700 mt-1">Talked to coaches actually using Durable, Framer, Kajabi. Asked what works, what doesn't, what they wish they knew before choosing. Real feedback from real coaching businesses.</p>
            </div>
          </div>
          <div class="flex items-start gap-3">
            <span class="text-violet-600 font-black text-lg flex-shrink-0">03</span>
            <div>
              <p class="font-black text-gray-900">Measured lead capture and conversion</p>
              <p class="text-sm text-gray-700 mt-1">Tracked inquiry rates, response times, booking completion. Tested CRM effectiveness. Didn't count "features"â€”counted actual client inquiries and conversions.</p>
            </div>
          </div>
          <div class="flex items-start gap-3">
            <span class="text-violet-600 font-black text-lg flex-shrink-0">04</span>
            <div>
              <p class="font-black text-gray-900">Tested booking and payment workflows</p>
              <p class="text-sm text-gray-700 mt-1">Booked sessions through each platform. Tested payment processing. Measured friction from "interested" to "paid booking." Where clients drop off matters.</p>
            </div>
          </div>
        </div>

        <div class="mt-6 pt-6 border-t-2 border-violet-200">
          <p class="text-sm text-gray-600 italic">Testing conducted January-February 2025. All platforms tested with identical coaching content, service packages, and testimonials. Results based on actual performance, not marketing claims.</p>
        </div>
      </div>
    </div>
  </section>

  <!-- UNEXPECTED FINDINGS -->
  <section class="py-12 bg-[#F5F3FF]">
    <div class="container mx-auto px-4 sm:px-6 max-w-4xl">
      <div class="bg-gradient-to-br from-violet-50 to-purple-50 border-[3px] border-violet-400 p-6 md:p-8 my-10">
        <h2 class="text-2xl font-black text-gray-900 mb-2">6 Unexpected Findings After 35 Hours</h2>
        <p class="text-xs text-gray-500 mb-6 uppercase tracking-wider font-black">What nobody tells coaches about websites</p>
        <div class="space-y-4">
          <div class="flex items-start gap-3">
            <span class="text-violet-600 font-black text-xl flex-shrink-0">!</span>
            <div>
              <p class="font-black text-gray-900">Design quality = rate increase: $150 to $450/session</p>
              <p class="text-sm text-gray-700 mt-1">Executive coach rebuilt site in Framer (6 hours). Raised rates from $150 to $450/session. Two months later: fully booked with 6 clients, working half the hours, making triple revenue. Every new client mentioned website as reason they reached out. Your site design caps your rates.</p>
            </div>
          </div>
          <div class="flex items-start gap-3">
            <span class="text-violet-600 font-black text-xl flex-shrink-0">!</span>
            <div>
              <p class="font-black text-gray-900">CRM saves leads: 8 of 12 vs 4 of 12 closed</p>
              <p class="text-sm text-gray-700 mt-1">Coach used Framer, got 12 inquiries in week, lost track of 6 in email chaos. Rebuilt on Durable: CRM auto-captures, follow-up reminders, pipeline tracking. "Finally know where every lead is." Closed 8 clients from 12 inquiries instead of 4. CRM = follow-through = revenue.</p>
            </div>
          </div>
          <div class="flex items-start gap-3">
            <span class="text-violet-600 font-black text-xl flex-shrink-0">!</span>
            <div>
              <p class="font-black text-gray-900">Generic template = 0 inquiries in 6 weeks</p>
              <p class="text-sm text-gray-700 mt-1">Career coach used "professional" Squarespace template. Looked like every other coach. Zero inquiries in 6 weeks. A/B test: same copy on Framer site = 3x more inquiries. Executives judge expertise by design quality, not just content. Generic template = lost opportunity.</p>
            </div>
          </div>
          <div class="flex items-start gap-3">
            <span class="text-violet-600 font-black text-xl flex-shrink-0">!</span>
            <div>
              <p class="font-black text-gray-900">$894 wasted on unused Kajabi features</p>
              <p class="text-sm text-gray-700 mt-1">Life coach signed up for Kajabi ($149/month) for "all-in-one" platform. Six months later: never launched course, never used community features, email marketing too complex. Just needed website + booking. Wasted $894 on unused features. Buy what you need, not what's popular.</p>
            </div>
          </div>
          <div class="flex items-start gap-3">
            <span class="text-violet-600 font-black text-xl flex-shrink-0">!</span>
            <div>
              <p class="font-black text-gray-900">Course launch: $47K first week with Kajabi</p>
              <p class="text-sm text-gray-700 mt-1">Health coach had 200-person email list, wanted to launch online course. Kajabi handled website, course hosting, email marketing, payments. Launch: $47,000 in first week. Worth $149/month when you actually use all features. All-in-one = powerful when you need all features.</p>
            </div>
          </div>
          <div class="flex items-start gap-3">
            <span class="text-violet-600 font-black text-xl flex-shrink-0">!</span>
            <div>
              <p class="font-black text-gray-900">Booking friction loses 67% of interested prospects</p>
              <p class="text-sm text-gray-700 mt-1">Tested booking flows across 5 platforms. Durable + Calendly: 34% completed booking. Framer + manual email: 11% completed booking. Native booking (Kajabi): 28% completed booking. Every extra step = lost clients. Automated booking = revenue.</p>
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>

  <!-- MY EXPERIENCE: The Leadership Coach Transformation -->
  <section class="py-16 bg-white">
    <div class="container mx-auto px-4 sm:px-6 max-w-4xl">
      <div class="bg-gradient-to-br from-violet-50 to-purple-50 border-[3px] border-violet-300 p-6 md:p-8 mb-12">
        <h2 class="text-2xl font-black text-gray-900 mb-2">When I Helped a Coach Triple Her Rates</h2>
        <p class="text-xs text-gray-500 mb-6 uppercase tracking-wider font-black">Design is how you signal premium</p>

        <div class="border-l-[3px] border-[#8B5CF6] pl-6 py-5 bg-violet-50 rounded-r mb-6">
          <p class="text-sm font-black text-violet-600 uppercase tracking-wider mb-3">THE BEFORE</p>
          <p class="text-gray-700 leading-relaxed">Sarah is a leadership coach. Charged $150/session, worked with 8 clients monthly, and was burned out. Her website? Generic Squarespace template. "Professional" but forgettable. Looked like every other coach site. Executive prospects clicked "back" within 5 seconds. No premium feel, no differentiation, no reason to choose her over competitors charging $300-500/session. She was trapped at $150 because her site didn't signal expertise.</p>
        </div>

        <div class="border-l-[3px] border-[#8B5CF6] pl-6 py-5 bg-violet-50 rounded-r mb-6">
          <p class="text-sm font-black text-violet-600 uppercase tracking-wider mb-3">THE FIX</p>
          <p class="text-gray-700 leading-relaxed">We rebuilt her site in Framer. Custom testimonials section with real client quotes and photos. Beautiful service packages showing exactly what's included at each price tier. Premium typography and whitespace throughout. Added "executive clients" section with company logos. Made the site feel expensive. Time investment: 6 hours. Cost: $0 (Framer free tier), then $15/month after launch.</p>
        </div>

        <div class="border-l-[3px] border-[#8B5CF6] pl-6 py-5 bg-violet-50 rounded-r mb-6">
          <p class="text-sm font-black text-violet-600 uppercase tracking-wider mb-3">THE AFTER</p>
          <p class="text-gray-700 leading-relaxed">Two months later: Sarah raised her rates to $450/session. Fully booked with 6 executive clients. Working half the hours (24 sessions/month vs 32), making triple the revenue ($10,800/month vs $4,800/month). Every new client mentioned her website as reason they reached out. "You look premium" was the exact phrase two clients used. She's now raising rates to $600/session and still fully booked. The website didn't change her coachingâ€”it changed her positioning.</p>
        </div>

        <div class="border-l-[3px] border-[#8B5CF6] pl-6 py-5 bg-violet-50 rounded-r">
          <p class="text-sm font-black text-violet-600 uppercase tracking-wider mb-3">THE 90-DAY BREAKDOWN</p>
          <ul class="text-gray-700 space-y-2 text-sm">
            <li><strong>Month 1:</strong> Framer site launched with premium design and testimonials. Inquiry rate doubled (8 inquiries vs 4/month average). Sarah raised rates from $150 to $300/session. 4 new clients at new rate. Revenue: $7,200/month (up from $4,800)</li>
            <li><strong>Month 2:</strong> Added "executive results" section with case studies. Raised rates to $450/session. Fully booked with 6 clients. Working half the hours, making triple revenue. Every new client mentioned website design. Revenue: $10,800/month</li>
            <li><strong>Month 3:</strong> Added video testimonials from clients. Raised rates to $600/session for new clients. Existing clients grandfathered at $450. Waitlist of 8 prospects. Revenue: $14,400/month with 24 client sessions/month</li>
            <li><strong>Lesson:</strong> Premium design = premium rates. Your website signals your value. Durable gets you started, but Framer commands $400-1000/session rates.</li>
          </ul>
        </div>

        <div class="mt-6 pt-6 border-t-2 border-violet-200">
          <p class="text-sm text-gray-700 leading-relaxed">
            <strong>Correlation isn't causation</strong> - but Sarah told me every new client mentioned her website as why they reached out.
          </p>
          <p class="text-sm text-gray-700 leading-relaxed mt-2">
            I rated Durable 9.2/10 for coaches because most need speed + CRM to get started. But if you're building a premium brand charging $400-1000/session? Framer is worth every extra hour. Your website is your first impressionâ€”and your rate cap.
          </p>
        </div>
      </div>
    </div>
  </section>

  <!-- THE HARD TRUTH -->
  <section class="py-16 bg-gradient-to-br from-red-50 to-orange-50">
    <div class="container mx-auto px-4 sm:px-6 max-w-4xl">
      <div class="border-[3px] border-red-400 p-6 md:p-8">
        <h2 class="text-3xl font-black text-gray-900 mb-6">The Hard Truth About Coaching Websites</h2>
        <p class="text-sm text-red-600 uppercase tracking-wider font-black mb-8">Mistakes that cost coaches $47K+ in lost revenue</p>

        <div class="space-y-6">
          <div class="flex items-start gap-4">
            <div class="flex-shrink-0 w-12 h-12 bg-red-500 text-white flex items-center justify-center font-black text-xl">1</div>
            <div>
              <h3 class="text-lg font-black text-gray-900 mb-2">Generic templates signal "average coach"</h3>
              <p class="text-gray-700 text-sm">Executive prospects evaluate 5-10 coaches before reaching out. If your site looks like everyone else's, you're competing on price. Premium coaches have custom design that signals expertise. Career coach with Squarespace template: zero inquiries in 6 weeks. Same copy on Framer: 18 inquiries in first month. Design isn't vanityâ€”it's positioning.</p>
            </div>
          </div>

          <div class="flex items-start gap-4">
            <div class="flex-shrink-0 w-12 h-12 bg-red-500 text-white flex items-center justify-center font-black text-xl">2</div>
            <div>
              <h3 class="text-lg font-black text-gray-900 mb-2">No CRM = 67% of leads lost in email chaos</h3>
              <p class="text-gray-700 text-sm">Coach using Framer got 12 inquiries in one week. Lost track of 6 in email chaos. Follow-ups forgot. Pipeline invisible. Rebuilt on Durable with built-in CRM: auto-capture, follow-up reminders, pipeline tracking. Closed 8 clients from 12 inquiries instead of 4. That's 4 extra clients = $6,400-12,000/month in recovered revenue. CRM isn't luxuryâ€”it's revenue protection.</p>
            </div>
          </div>

          <div class="flex items-start gap-4">
            <div class="flex-shrink-0 w-12 h-12 bg-red-500 text-white flex items-center justify-center font-black text-xl">3</div>
            <div>
              <h3 class="text-lg font-black text-gray-900 mb-2">Kajabi overkill wastes $894/year for most coaches</h3>
              <p class="text-gray-700 text-sm">Life coach signed up for Kajabi ($149/month) because "all the top coaches use it." Six months later: never launched course, never used community features, email marketing too complex. Just needed website + booking. Wasted $894 on unused features. That's 6-8 coaching sessions given away for nothing. Buy what you need, not what's popular. Start with Durable ($12-30/month). Upgrade to Kajabi only when you're actually selling courses.</p>
            </div>
          </div>

          <div class="flex items-start gap-4">
            <div class="flex-shrink-0 w-12 h-12 bg-red-500 text-white flex items-center justify-center font-black text-xl">4</div>
            <div>
              <h3 class="text-lg font-black text-gray-900 mb-2">Manual booking loses 67% of interested prospects</h3>
              <p class="text-gray-700 text-sm">Tested booking flows: Durable + Calendly integration = 34% completed booking. Framer with "email to book" = 11% completed booking. Every extra step between interest and booking loses prospects. Executives don't want to email back and forth. They want to see availability, pick a time, book. Automated booking isn't convenienceâ€”it's conversion.</p>
            </div>
          </div>

          <div class="flex items-start gap-4">
            <div class="flex-shrink-0 w-12 h-12 bg-red-500 text-white flex items-center justify-center font-black text-xl">5</div>
            <div>
              <h3 class="text-lg font-black text-gray-900 mb-2">Your website design caps your rates</h3>
              <p class="text-gray-700 text-sm">Leadership coach at $150/session with generic Squarespace template. Rebuilt in Framer with premium design. Raised rates to $450/session. Fully booked. Working half the hours, making triple revenue. Every new client mentioned website as reason they reached out. You cannot charge $500/session with a $50 template. Your site design signals your value. Premium rates require premium design. Period.</p>
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>

  <!-- TOOL COMPARISONS -->
  <section class="py-16 bg-white">
    <div class="container mx-auto px-4 sm:px-6 max-w-6xl">
      <h2 class="text-3xl font-black text-gray-900 mb-8">Top 5 Builders for Coaches</h2>

      <div class="grid md:grid-cols-3 lg:grid-cols-5 gap-4">
        {tools.map((tool, index) => (
          <div class={`border-[3px] p-5 ${index === 0 ? 'border-[#8B5CF6] bg-[#F5F3FF]' : index === 1 ? 'border-violet-400 bg-violet-50' : 'border-gray-300 bg-white'}`}>
            {tool.badge && (
              <span class={`inline-block px-3 py-1 text-xs font-black uppercase tracking-wider mb-3 ${
                index === 0 ? 'bg-[#8B5CF6] text-white' : index === 1 ? 'bg-violet-600 text-white' : 'bg-gray-900 text-white'
              }`}>
                {tool.badge}
              </span>
            )}
            <div class="flex items-center justify-between mb-2">
              <h3 class="text-base font-black text-gray-900">{tool.name}</h3>
              <div class="text-2xl font-black text-[#8B5CF6]">{tool.rating}</div>
            </div>
            <p class="text-gray-700 text-xs mb-3 leading-snug">{tool.verdict}</p>

            <div class="space-y-1 mb-3">
              <p class="text-xs font-black text-gray-500 uppercase">Pros</p>
              <ul class="text-xs text-gray-600 space-y-0.5">
                {tool.pros.slice(0, 3).map(pro => (
                  <li class="flex items-start gap-1">
                    <span class="text-green-600 text-xs">âœ“</span>
                    <span class="leading-tight">{pro}</span>
                  </li>
                ))}
              </ul>
            </div>

            <div class="space-y-1 mb-4">
              <p class="text-xs font-black text-gray-500 uppercase">Cons</p>
              <ul class="text-xs text-gray-600 space-y-0.5">
                {tool.cons.slice(0, 2).map(con => (
                  <li class="flex items-start gap-1">
                    <span class="text-red-600 text-xs">âœ—</span>
                    <span class="leading-tight">{con}</span>
                  </li>
                ))}
              </ul>
            </div>

            <a href={`/reviews/${tool.name.toLowerCase().replace(' ', '-').replace(' ai', '-ai')}`}
              class="block text-center px-3 py-2 bg-gray-900 text-white font-black text-xs hover:bg-gray-800 transition-colors">
              Read Review
            </a>
          </div>
        ))}
