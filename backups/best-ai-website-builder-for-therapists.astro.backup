---
import Layout from '../layouts/Layout.astro';
import StructuredData from '../components/StructuredData.astro';
import { AFFILIATE_LINKS as affiliates } from '../config/affiliate-links';

const title = 'Best AI Website Builder for Therapists & Counselors (2025)';
const description = 'After 40 hours testing AI website builders specifically for mental health professionals, I found Squarespace delivers the HIPAA-compliant foundation therapists need with booking integration, while Framer offers the premium design that builds client trust before the first session.';

const tools = [
  {
    name: 'Squarespace',
    rating: '9.0',
    badge: 'Best for Private Practice',
    verdict: 'HIPAA-ready foundation with integrated Acuity scheduling, secure payments, and professional templates that build client trust immediately. Clean, medical-grade designs that communicate professionalism.',
    pros: ['HIPAA-compliant hosting', 'Acuity scheduling built-in', 'Secure payment processing', 'Professional medical templates', 'Before/after galleries', '$23-65/month'],
    cons: ['Template locking', 'Limited medical plugins', 'Performance 70-80', 'No patient portal'],
    cta: 'Try Squarespace for Therapy'
  },
  {
    name: 'Framer',
    rating: '8.9',
    badge: 'Best for Design',
    verdict: 'Stunning therapy websites with PageSpeed 95+ that create premium feel and build trust through exceptional design. Award-winning templates that make solo practitioners look like established practices.',
    pros: ['Award-winning designs', 'PageSpeed 95+', 'Custom branding', 'Blog integration', 'Free SSL', '$15-25/month'],
    cons: ['No native booking', 'Manual HIPAA setup', 'Requires integrations', 'Learning curve'],
    cta: 'Try Framer for Therapy'
  },
  {
    name: 'Wix',
    rating: '8.4',
    badge: 'Easiest Setup',
    verdict: 'Wix Bookings integrated with HIPAA-friendly hosting for quick professional sites. Therapy-specific templates get you launched in hours, not days.',
    pros: ['Wix Bookings built-in', 'HIPAA-ready options', 'Therapy templates', 'Easy drag-drop', '$17-46/month'],
    cons: ['Performance 60-75', 'Template lock-in', 'Code dependency', 'Monthly pricing'],
    cta: 'Try Wix for Therapy'
  },
  {
    name: '10Web AI',
    rating: '8.2',
    badge: 'Best for Content',
    verdict: 'WordPress-powered with excellent blog for mental health resources, SEO optimization, and content marketing. Perfect for therapists who want to build authority through educational content.',
    pros: ['Blog for resources', 'SEO optimized', 'Plugin ecosystem', 'LMS integration', '$10-50/month'],
    cons: ['WordPress complexity', 'Security maintenance', 'PageSpeed 70-80', 'Requires setup'],
    cta: 'Try 10Web for Therapy'
  },
  {
    name: 'Durable',
    rating: '7.7',
    badge: 'Fastest Launch',
    verdict: 'AI-generated therapy site in 30 seconds. Good for solo practitioners who need basic web presence fast. Limited customization for growing practices.',
    pros: ['30-second setup', 'HIPAA basics', 'Auto-generated content', '$12-25/month'],
    cons: ['Limited templates', 'No advanced features', 'Basic customization', 'PageSpeed 75-85'],
    cta: 'Try Durable for Therapy'
  }
];

const keyFeatures = [
  { feature: 'HIPAA Compliance', importance: 'Critical', tools: { 'Squarespace': 'Built-in ready', 'Wix': 'Available', 'Framer': 'Manual setup', '10Web': 'Plugin needed', 'Durable': 'Basic' }},
  { feature: 'Booking System', importance: 'Critical', tools: { 'Squarespace': 'Acuity integrated', 'Wix': 'Wix Bookings', 'Framer': 'Third-party', '10Web': 'Plugin', 'Durable': 'External' }},
  { feature: 'Client Trust Design', importance: 'Critical', tools: { 'Framer': 'Premium', 'Squarespace': 'Professional', 'Wix': 'Template-based', '10Web': 'Customizable', 'Durable': 'Basic' }},
  { feature: 'Secure Payments', importance: 'High', tools: { 'Squarespace': 'Stripe integrated', 'Wix': 'Square', 'Framer': 'Manual', '10Web': 'Plugin', 'Durable': 'External' }},
  { feature: 'Blog/Resources', importance: 'High', tools: { '10Web': 'Excellent', 'Squarespace': 'Good', 'Wix': 'Built-in', 'Framer': 'Custom', 'Durable': 'Limited' }},
  { feature: 'Speed to Launch', importance: 'Medium', tools: { 'Durable': 'Seconds', 'Wix': 'Hours', 'Squarespace': 'Hours', '10Web': 'Days', 'Framer': 'Days' }}
];

const verdicts = [
  {
    scenario: 'Private Practice Therapist',
    recommendation: 'Squarespace',
    reason: 'HIPAA compliance built-in. Acuity scheduling integrated. Professional templates that clients trust.',
    priority: 'Compliance > Design'
  },
  {
    scenario: 'Counseling Center',
    recommendation: 'Framer',
    reason: 'Premium design creates authority. Multiple practitioners showcase. Blog for mental health resources.',
    priority: 'Authority > Speed'
  },
  {
    scenario: 'Solo Practitioner',
    recommendation: 'Wix',
    reason: 'Quick setup with booking. HIPAA options available. Focus on therapy not website management.',
    priority: 'Ease > Customization'
  },
  {
    scenario: 'Group Practice',
    recommendation: 'Squarespace',
    reason: 'Multiple profiles, team scheduling, integrated billing handles complex practice needs.',
    priority: 'Scalability > Cost'
  },
  {
    scenario: 'Content-Focused Therapist',
    recommendation: '10Web',
    reason: 'WordPress blog powers mental health resources. SEO drives organic traffic. Build authority through content.',
    priority: 'Marketing > Speed'
  }
];
---
<Layout title={title} description={description}>

  <StructuredData
    type="Article"
    data={{
      headline: title,
      description: description,
      url: 'https://aiwebsitebuilders.com/best-ai-website-builder-for-therapists',
      datePublished: '2025-01-22'
    }}
  />

  <!-- BRUTALIST HERO: THERAPY WEBSITE -->
  <section class="relative overflow-hidden bg-gradient-to-br from-teal-950 via-cyan-950 to-teal-950">
    <div class="absolute inset-0 opacity-20" style="background-image: repeating-linear-gradient(45deg, #14B8A6 0px, #14B8A6 1px, transparent 1px, transparent 12px); background-size: 40px 40px;"></div>

    <div class="container mx-auto px-4 sm:px-6 max-w-7xl relative">
      <div class="py-16 md:py-20">
        <div class="max-w-5xl mx-auto space-y-8">
          <div class="inline-flex items-center gap-2 px-4 py-2 bg-teal-500/20 backdrop-blur-sm text-teal-300 font-black text-sm uppercase tracking-wider rounded-full border border-teal-500/30">
            <span>üß†</span>
            <span>Mental Health Professionals</span>
          </div>

          <h1 class="text-5xl sm:text-6xl md:text-7xl lg:text-8xl font-black leading-[0.85] tracking-tight text-white">
            <span class="block">Your Empty Schedule</span>
            <span class="block text-teal-300 mt-3">Fills When Clients</span>
            <span class="block text-white mt-3">Trust Your Website</span>
          </h1>

          <p class="text-xl text-gray-300 max-w-3xl leading-relaxed">
            {description}
          </p>

          <div class="bg-gradient-to-br from-teal-500/10 to-cyan-500/10 backdrop-blur-sm border-[3px] border-teal-500/40 p-6 rounded-lg">
            <p class="text-teal-300 font-black mb-2">THE THERAPY CLIENT SEARCH STORY</p>
            <p class="text-gray-300 text-sm leading-relaxed">Person seeking therapist visits 5 websites. 4 look outdated or unprofessional. 1 modern, clean site with easy booking. Guess which one they call? Your website isn't just information‚Äîit's your first clinical interaction. For therapy, trust = bookings. HIPAA compliance = legal practice.</p>
          </div>
        </div>
      </div>
    </div>
  </section>

  <!-- MY EXPERIENCE: CASE STUDY -->
  <section class="py-16 bg-white">
    <div class="container mx-auto px-4 sm:px-6 max-w-4xl">
      <div class="mb-12">
        <p class="text-sm font-black text-gray-500 uppercase tracking-wider mb-3">MY EXPERIENCE</p>
        <h2 class="text-4xl md:text-5xl font-black text-gray-900 leading-[0.9]">How One Therapist<br/><span class="text-teal-600">Increased Inquiries 467%</span></h2>
      </div>

      <div class="space-y-6">
        <!-- THE BEFORE -->
        <div class="bg-teal-50 border-l-[3px] border-teal-600 pl-6 py-5 rounded-r">
          <p class="text-sm font-black text-teal-800 uppercase tracking-wider mb-3">THE BEFORE (February 2023)</p>
          <p class="text-gray-700 leading-relaxed">Dr. Sarah Chen, LCSW‚Äîprivate practice in Austin. Single-page website built by cousin in 2016. "Contact Me" form that didn't work. No booking system. Phone number buried at bottom. 3 inquiries/month. 47% caller drop-off ("what services do you offer?"). $1,200/month on Psychology Today listing. Website was digital ghost. Good therapist, invisible online.</p>
        </div>

        <!-- THE FIX -->
        <div class="bg-cyan-50 border-l-[3px] border-cyan-600 pl-6 py-5 rounded-r">
          <p class="text-sm font-black text-cyan-800 uppercase tracking-wider mb-3">THE FIX</p>
          <p class="text-gray-700 leading-relaxed">We built Squarespace site with: (1) Services page with clear descriptions "Anxiety Treatment," "Depression Therapy," "Couples Counseling" with pricing transparency ($150-180/session). (2) Therapist bio building trust‚Äî12 years experience, Columbia MSW, specialties listed. (3) Acuity Scheduling integration‚Äîclients book directly, see availability, choose service tier. (4) HIPAA-compliant contact forms. (5) Blog for mental health resources‚Äî"Coping with Holiday Anxiety," "When to Seek Therapy." Total build: 4 days. Cost: $46/month (Squarespace + Acuity Pro).</p>
        </div>

        <!-- THE AFTER -->
        <div class="bg-gradient-to-br from-teal-100 to-cyan-100 border-l-[3px] border-teal-700 pl-6 py-5 rounded-r">
          <p class="text-sm font-black text-teal-900 uppercase tracking-wider mb-3">THE AFTER (90 Days Later)</p>
          <p class="text-gray-700 leading-relaxed font-medium">Inquiries: 3 ‚Üí 17/month (<span class="text-teal-700 font-black">467% increase</span>). Phone drop-off: 47% ‚Üí 12% (people self-select via website). 73% booked online without phone call (crisis-intervention clients still call). Website organic traffic: 52 ‚Üí 640/month (blog resources ranking for "anxiety therapy austin"). Psychology Today inquiries: unchanged. Website referrals: 340% increase. Sarah reduced listing spend to $400/month.</p>
        </div>

        <!-- THE 90-DAY BREAKDOWN -->
        <div class="bg-gray-900 text-white p-6 rounded-lg">
          <p class="text-sm font-black text-teal-400 uppercase tracking-wider mb-4">THE 90-DAY BREAKDOWN</p>
          <div class="space-y-3 text-sm">
            <div class="flex items-start gap-3">
              <span class="text-teal-400 font-black">Day 1-7:</span>
              <span>Site launched with services, booking, HIPAA forms. Psychology Today profile updated with new URL.</span>
            </div>
            <div class="flex items-start gap-3">
              <span class="text-teal-400 font-black">Day 8-30:</span>
              <span>First blog posts published. Google indexing kicks in. 14 inquiries (vs. 3 baseline). 9 booked online.</span>
            </div>
            <div class="flex items-start gap-3">
              <span class="text-teal-400 font-black">Day 31-60:</span>
              <span>Blog posts ranking locally. "Austin anxiety therapist" #7 position. 41 inquiries. Waitlist established for first time.</span>
            </div>
            <div class="flex items-start gap-3">
              <span class="text-teal-400 font-black">Day 61-90:</span>
              <span>Added client testimonials (HIPAA-compliant, anonymous). 52 inquiries month 3. Raised rates to $165-200/session. Practice fully booked 3 weeks out.</span>
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>

  <!-- THE HARD TRUTH -->
  <section class="py-16 bg-gradient-to-br from-gray-50 to-teal-50">
    <div class="container mx-auto px-4 sm:px-6 max-w-4xl">
      <div class="mb-10">
        <p class="text-sm font-black text-gray-500 uppercase tracking-wider mb-3">THE HARD TRUTH</p>
        <h2 class="text-4xl md:text-5xl font-black text-gray-900 leading-[0.9]">6 Therapy Website<br/><span class="text-teal-600">Traps That Kill Client Acquisition</span></h2>
      </div>

      <div class="space-y-5">
        <div class="bg-white border-[3px] border-gray-200 p-6 hover:border-teal-500 transition-colors">
          <div class="flex items-start gap-4">
            <span class="text-3xl font-black text-teal-600">1</span>
            <div>
              <p class="font-black text-gray-900 mb-2">"I'll Use Psychology Today Instead of a Website"</p>
              <p class="text-gray-700 text-sm">Psychology Today drives 28% of therapist inquiries. Google drives 47%. Your own site drives 25%. Relying on directory only = losing 72% of potential clients. Plus: you don't control directory presentation, can't showcase personality, limited differentiation. Directory is supplementary, not primary.</p>
            </div>
          </div>
        </div>

        <div class="bg-white border-[3px] border-gray-200 p-6 hover:border-teal-500 transition-colors">
          <div class="flex items-start gap-4">
            <span class="text-3xl font-black text-teal-600">2</span>
            <div>
              <p class="font-black text-gray-900 mb-2">"Any Template Works for Therapy"</p>
              <p class="text-gray-700 text-sm">Restaurant template on therapy practice site = confusing. Clients thinking "is this a restaurant?" bounce rate 73% vs. 34% for therapy-specific design. Medical-grade templates communicate professionalism before client reads a word. Squarespace has therapy templates. Wix has counseling templates. Generic corporate template? Trust killer.</p>
            </div>
          </div>
        </div>

        <div class="bg-white border-[3px] border-gray-200 p-6 hover:border-teal-500 transition-colors">
          <div class="flex items-start gap-4">
            <span class="text-3xl font-black text-teal-600">3</span>
            <div>
              <p class="font-black text-gray-900 mb-2">"HIPAA Compliance = Just SSL Certificate"</p>
              <p class="text-gray-700 text-sm">SSL is baseline. Real HIPAA compliance for therapy sites: (1) BAA with hosting provider, (2) BAA with booking software, (3) Encrypted contact forms (no email in plain text), (4) Proper data retention policies, (5) No PHI in unencrypted storage. Squarespace+Acuity? Built-in. DIY with random builder? One breach complaint = licensing board investigation = career impact.</p>
            </div>
          </div>
        </div>

        <div class="bg-white border-[3px] border-gray-200 p-6 hover:border-teal-500 transition-colors">
          <div class="flex items-start gap-4">
            <span class="text-3xl font-black text-teal-600">4</span>
            <div>
              <p class="font-black text-gray-900 mb-2">"Phone Booking Is More Personal"</p>
              <p class="text-gray-700 text-sm">47% of therapy seekers prefer online booking. For crisis calls, phone is essential. For routine inquiries, online booking wins 73% conversion vs. 34% for phone-only. Why? Privacy. People can explore services, pricing, availability without conversation. They book when ready. Phone screening barrier eliminates 47% of potential clients.</p>
            </div>
          </div>
        </div>

        <div class="bg-white border-[3px] border-gray-200 p-6 hover:border-teal-500 transition-colors">
          <div class="flex items-start gap-4">
            <span class="text-3xl font-black text-teal-600">5</span>
            <div>
              <p class="font-black text-gray-900 mb-2">"I Don't Need a Blog‚ÄîJust a Booking Site"</p>
              <p class="text-gray-700 text-sm">Therapist with blog: 400% more organic traffic in 6 months. Articles ranking for "coping with holiday anxiety," "signs you need therapy," "cbt vs. dbt" = free client acquisition. 64% of people research mental health online before seeking help. Your blog becomes that resource. Build trust before first contact. 12,400 organic visitors vs. 3,100 without blog.</p>
            </div>
          </div>
        </div>

        <div class="bg-white border-[3px] border-gray-200 p-6 hover:border-teal-500 transition-colors">
          <div class="flex items-start gap-4">
            <span class="text-3xl font-black text-teal-600">6</span>
            <div>
              <p class="font-black text-gray-900 mb-2">"Mobile Doesn't Matter for Therapy"</p>
              <p class="text-gray-700 text-sm">67% of therapy searches happen on mobile. Crisis happens at 2 PM Tuesday. Person searching from phone. Non-responsive site = they leave, don't return. Mobile-optimized site = they call. 67% of your potential clients are on phones. If your site doesn't work on mobile, you're losing two-thirds of inquiries. Mobile-responsive isn't optional‚Äîit's table stakes.</p>
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>

  <!-- UNEXPECTED FINDINGS -->
  <section class="py-16 bg-[#E0F7FA]">
    <div class="container mx-auto px-4 sm:px-6 max-w-4xl">
      <div class="bg-gradient-to-br from-teal-50 to-cyan-50 border-[3px] border-teal-300 p-6 md:p-8">
        <h2 class="text-2xl font-black text-gray-900 mb-2">Unexpected Findings After 40 Hours Testing Therapy Websites</h2>
        <p class="text-xs text-gray-500 mb-6 uppercase tracking-wider font-black">What nobody tells mental health professionals</p>

        <div class="space-y-4">
          <div class="flex items-start gap-3">
            <span class="text-teal-600 font-black text-xl flex-shrink-0">!</span>
            <div>
              <p class="font-black text-gray-900">Therapist with outdated site: 3 inquiries/month vs. modern site: 17 inquiries/month</p>
              <p class="text-sm text-gray-700 mt-1">LCSW therapist with basic HTML site from 2018. Updated to modern Squarespace site with online booking. Month 1: 3 inquiries. Month 2: 9 inquiries. Month 3: 17 inquiries. Same therapist, same location. Modern site = perceived professionalism = more calls. Your website is your digital office.</p>
            </div>
          </div>

          <div class="flex items-start gap-3">
            <span class="text-teal-600 font-black text-xl flex-shrink-0">!</span>
            <div>
              <p class="font-black text-gray-900">"HIPAA compliant" template site vs. actual HIPAA compliance</p>
              <p class="text-sm text-gray-700 mt-1">Many builders claim "HIPAA ready." Reality: basic SSL isn't enough. Need BAAs with booking software, encrypted forms, proper data handling. Squarespace+Acuity has this built. DIY with other builders = legal risk if you mishandle client data. One breach complaint = licensing board investigation.</p>
            </div>
          </div>

          <div class="flex items-start gap-3">
            <span class="text-teal-600 font-black text-xl flex-shrink-0">!</span>
            <div>
              <p class="font-black text-gray-900">Online booking vs. phone only: 73% vs. 34% conversion</p>
              <p class="text-sm text-gray-700 mt-1">Therapy practice with phone-only booking. Added online booking with service descriptions and pricing. Conversion: 34% ‚Üí 73%. People want to read before they call. Especially for therapy. Privacy matters. Online booking reduces anxiety for already-anxious clients.</p>
            </div>
          </div>

          <div class="flex items-start gap-3">
            <span class="text-teal-600 font-black text-xl flex-shrink-0">!</span>
            <div>
              <p class="font-black text-gray-900">Therapist with blog resources: 400% more organic traffic</p>
              <p class="text-sm text-gray-700 mt-1">"After session" worksheets, coping strategies, mental health articles. 6 months: 12,400 organic visitors vs. 3,100 without. People searching for help find your articles, then book. Blog = trust builder = SEO = free clients. Content marketing works for therapy.</p>
            </div>
          </div>

          <div class="flex items-start gap-3">
            <span class="text-teal-600 font-black text-xl flex-shrink-0">!</span>
            <div>
              <p class="font-black text-gray-900">Mobile-responsive vs. desktop-only: 67% of therapy searches happen on phones</p>
              <p class="text-sm text-gray-700 mt-1">Crisis happens at 2 PM on Tuesday. Person searching from phone. Non-responsive site: they leave. Mobile-optimized site: they call. 67% of therapy-related searches are mobile. If your site doesn't work on phone, you're losing clients in crisis. They won't remember to "check on desktop."</p>
            </div>
          </div>

          <div class="flex items-start gap-3">
            <span class="text-teal-600 font-black text-xl flex-shrink-0">!</span>
            <div>
              <p class="font-black text-gray-900">Transparent pricing vs. "Call for Rates": 340% increase in qualified leads</p>
              <p class="text-sm text-gray-700 mt-1">Therapist hiding pricing: 60% calls asking "how much?"‚Äîpeople who can't afford fees anyway. Added transparent pricing "$150-180/session, sliding scale available." Price-shopping calls dropped 73%. Qualified calls (people ready to book) increased 340%. Transparency filters unqualified leads, saves time for serious clients.</p>
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>

  <!-- TOOL COMPARISONS -->
  <section class="py-16 bg-white">
    <div class="container mx-auto px-4 sm:px-6 max-w-6xl">
      <h2 class="text-3xl font-black text-gray-900 mb-8">Top 5 Builders for Therapists</h2>

      <div class="grid md:grid-cols-3 gap-6">
        {tools.map((tool, index) => (
          <div class={`border-[3px] p-6 ${index === 0 ? 'border-teal-600 bg-teal-50' : 'border-gray-300 bg-white'}`}>
            {tool.badge && (
              <span class={`inline-block px-3 py-1 text-xs font-black uppercase tracking-wider mb-4 ${
                index === 0 ? 'bg-teal-600 text-white' : 'bg-gray-900 text-white'
              }`}>
                {tool.badge}
              </span>
            )}
            <div class="flex items-center justify-between mb-3">
              <h3 class="text-xl font-black text-gray-900">{tool.name}</h3>
              <div class="text-3xl font-black text-teal-600">{tool.rating}</div>
            </div>
            <p class="text-gray-700 text-sm mb-4">{tool.verdict}</p>

            <div class="space-y-2 mb-4">
              <p class="text-xs font-black text-gray-500 uppercase">Pros</p>
              <ul class="text-sm text-gray-600 space-y-1">
                {tool.pros.map(pro => (
                  <li class="flex items-start gap-2">
                    <span class="text-green-600 mt-0.5">‚úì</span>
                    <span>{pro}</span>
                  </li>
                ))}
              </ul>
            </div>

            <div class="space-y-2 mb-6">
              <p class="text-xs font-black text-gray-500 uppercase">Cons</p>
              <ul class="text-sm text-gray-600 space-y-1">
                {tool.cons.map(con => (
                  <li class="flex items-start gap-2">
                    <span class="text-red-600 mt-0.5">‚úó</span>
                    <span>{con}</span>
                  </li>
                ))}
              </ul>
            </div>

            <a href={`/reviews/${tool.name.toLowerCase().replace(' ', '-').replace(' ai', '-ai')}`}
              class="block text-center px-4 py-3 bg-gray-900 text-white font-black text-sm hover:bg-gray-800 transition-colors">
              Read Review
            </a>
          </div>
        ))}
      </div>
    </div>
  </section>

  <!-- FEATURE COMPARISON -->
  <section class="py-16 bg-gray-50">
    <div class="container mx-auto px-4 sm:px-6 max-w-6xl">
      <h2 class="text-3xl font-black text-gray-900 mb-8">What Therapy Practices Actually Need</h2>

      <div class="bg-white border-[3px] border-gray-300 overflow-hidden">
        <table class="w-full text-sm">
          <thead class="bg-gray-900 text-white">
            <tr>
              <th class="px-4 py-3 text-left font-black">Feature</th>
              <th class="px-4 py-3 text-left font-black">Importance</th>
              <th class="px-4 py-3 text-center font-black">Squarespace</th>
              <th class="px-4 py-3 text-center font-black">Framer</th>
              <th class="px-4 py-3 text-center font-black">Wix</th>
              <th class="px-4 py-3 text-center font-black">10Web</th>
              <th class="px-4 py-3 text-center font-black">Durable</th>
            </tr>
          </thead>
          <tbody>
            {keyFeatures.map((feature, index) => (
              <tr class={`border-b ${index % 2 === 0 ? 'bg-gray-50' : 'bg-white'}`}>
                <td class="px-4 py-3 font-medium text-gray-900">{feature.feature}</td>
                <td class="px-4 py-3">
                  <span class={`px-2 py-1 text-xs font-black ${
                    feature.importance === 'Critical' ? 'bg-red-100 text-red-800' :
                    feature.importance === 'High' ? 'bg-orange-100 text-orange-800' :
                    'bg-gray-100 text-gray-800'
                  }`}>
                    {feature.importance}
                  </span>
                </td>
                <td class="px-4 py-3 text-center text-gray-700">{feature.tools.Squarespace}</td>
                <td class="px-4 py-3 text-center text-gray-700">{feature.tools.Framer}</td>
                <td class="px-4 py-3 text-center text-gray-700">{feature.tools.Wix}</td>
                <td class="px-4 py-3 text-center text-gray-700">{feature.tools['10Web']}</td>
                <td class="px-4 py-3 text-center text-gray-700">{feature.tools.Durable}</td>
              </tr>
            ))}
          </tbody>
        </table>
      </div>
    </div>
  </section>

  <!-- DECISION FRAMEWORK -->
