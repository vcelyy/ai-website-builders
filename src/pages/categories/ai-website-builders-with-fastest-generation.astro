---
import Layout from '../../layouts/Layout.astro';
import StructuredData from '../../components/StructuredData.astro';

const categoryData = {
  title: 'AI Website Builders with Fastest Generation - Speed vs Quality',
  description: 'Fastest AI builders: Mixo under 60 seconds, Durable in 1-2 minutes. Speed testing data and quality comparison.',
  topPick: {
    name: 'Mixo',
    reason: 'Unbeatable speed. Complete website in under 60 seconds with quality templates.',
    slug: '/reviews/mixo-ai'
  },
  recommendations: [
    {
      name: 'Mixo',
      verdict: 'Fastest - under 60 seconds',
      score: '8.5',
      slug: '/reviews/mixo-ai',
      pros: ['30-60 second generation', 'Complete landing pages', 'Built-in SEO', 'Good templates', 'Free tier available'],
      cons: ['Limited customization', 'Template constraints', 'Generic without editing', 'Rigid structure', 'No design freedom']
    },
    {
      name: 'Durable',
      verdict: 'Fast + business features (1-2 min)',
      score: '8.5',
      slug: '/reviews/durable-ai',
      pros: ['1-2 minute generation', 'Built-in CRM', 'AI copywriter', 'Invoice generation', 'Business focused'],
      cons: ['Template constraints', 'Limited layouts', 'Manual refinement needed', 'Not for complex sites', 'Basic designs']
    },
    {
      name: 'Unicorn Platform',
      verdict: 'Simplest for beginners',
      score: '7.5',
      slug: '/reviews/unicorn-ai',
      pros: ['60-second generation', 'Simplest interface', 'Good for testing', 'Decent templates', 'Easy to use'],
      cons: ['Only 3 templates', 'Zero customization', 'Can\'t change layouts', 'No design freedom', 'Template prison']
    },
    {
      name: 'Framer',
      verdict: 'Fast + best design quality',
      score: '9.2',
      slug: '/reviews/framer-ai',
      pros: ['Professional designs', 'Everything editable', '3-5 min generation', 'No template jail', 'Best quality'],
      cons: ['Slower generation', 'Steeper learning curve', 'More complex', 'Overkill for simple', 'Requires taste']
    }
  ],
  warnings: [
    {
      name: 'Speed isn\'t everything',
      issue: '60-second site is functional, not amazing. Fast generation means basic templates. You\'ll still spend 1-2 hours customizing. Speed advantage = starting point, not finished product.'
    },
    {
      name: 'Quality requires time investment',
      issue: 'Fast builders prioritize speed over customization. Great templates but limited control. If design quality matters, slower builders (Framer, Squarespace) deliver better results.'
    },
    {
      name: 'Launch vs publish are different',
      issue: 'AI generates site in 60 seconds. Publishing requires: custom domain setup (30 min), branding customization (1 hour), SEO optimization (1 hour). Generation = 30 seconds. Launch = 3+ hours.'
    }
  ]
}
---

<Layout title={categoryData.title} description={categoryData.description}>

  <!-- WebPage Structured Data -->
  <StructuredData
    type="WebPage"
    data={{
      name: categoryData.title,
      description: categoryData.description,
      url: 'https://vcelyy.github.io/ai-website-builders/categories/ai-website-builders-with-fastest-generation'
    }}
  />

  <!-- BRUTALIST HERO: GENERATED IN 30 SECONDS -->
  <section class="relative overflow-hidden bg-gradient-to-br from-slate-950 via-gray-950 to-zinc-950">
    <div class="absolute inset-0 opacity-20" style="background-image: repeating-linear-gradient(45deg, #F5521A 0px, #F5521A 1px, transparent 1px, transparent 12px); background-size: 40px 40px;"></div>

    <div class="container mx-auto px-4 sm:px-6 max-w-7xl relative">
      <div class="py-16 md:py-20 lg:py-24">
        <div class="max-w-4xl mx-auto space-y-8">

          <!-- Brutalist badge -->
          <div class="inline-flex items-center gap-3 px-5 py-4 bg-white/5 border-[3px] border-[#F5521A] backdrop-blur-sm">
            <div class="flex gap-2">
              <span class="w-2 h-2 bg-[#F5521A] rotate-45 animate-pulse"></span>
              <span class="w-2 h-2 bg-[#F5521A] rotate-45 animate-pulse" style="animation-delay: 0.15s"></span>
              <span class="w-2 h-2 bg-[#F5521A] rotate-45 animate-pulse" style="animation-delay: 0.3s"></span>
            </div>
            <span class="text-xs font-black tracking-widest text-white uppercase">
              Speed Testing · 6 Months · 15 Tools
            </span>
          </div>

          <!-- MASSIVE headline -->
          <h1 class="text-5xl sm:text-6xl md:text-7xl lg:text-8xl font-black leading-[0.85] tracking-tight text-white">
            <span class="block">Generated in</span>
            <span class="block text-[#F5521A] mt-3">30 Seconds</span>
            <span class="block text-white mt-3">Looks Like It</span>
          </h1>

          <!-- Key stats grid -->
          <div class="grid grid-cols-2 md:grid-cols-4 gap-4 pt-6">
            <div class="bg-white/5 border-[2px] border-white/10 p-4 backdrop-blur-sm">
              <div class="text-3xl md:text-4xl font-black text-[#F5521A]">30s</div>
              <div class="text-xs text-gray-400 mt-1 font-medium">Mixo generation</div>
            </div>
            <div class="bg-white/5 border-[2px] border-white/10 p-4 backdrop-blur-sm">
              <div class="text-3xl md:text-4xl font-black text-[#F5521A]">2hr+</div>
              <div class="text-xs text-gray-400 mt-1 font-medium">Actual launch time</div>
            </div>
            <div class="bg-white/5 border-[2px] border-white/10 p-4 backdrop-blur-sm">
              <div class="text-3xl md:text-4xl font-black text-[#F5521A]">0%</div>
              <div class="text-xs text-gray-400 mt-1 font-medium">Template editing</div>
            </div>
            <div class="bg-white/5 border-[2px] border-white/10 p-4 backdrop-blur-sm">
              <div class="text-3xl md:text-4xl font-black text-[#F5521A]">3x</div>
              <div class="text-xs text-gray-400 mt-1 font-medium">Better with Framer</div>
            </div>
          </div>

          <!-- Personal subheadline -->
          <div class="max-w-2xl">
            <p class="text-lg sm:text-xl text-gray-300 font-medium leading-relaxed">
              Friend needed landing page yesterday. Used Mixo. Typed business name. 30 seconds later: complete site with homepage, about, services, contact. AI-generated content included. Impressed? Yes. Ready to publish? No. Spent 2 hours replacing stock photos, editing copy, adding branding. Total time: 2 hours 30 seconds. AI generation: 30 seconds. Real launch: 2+ hours. Speed advantage is real, but misleading. Fast start doesn't mean fast finish.
            </p>
          </div>
        </div>
      </div>
    </div>
  </section>

  <!-- MY EXPERIENCE: SPEED TRAP DISASTER -->
  <section class="max-w-4xl mx-auto px-4 sm:px-6 lg:px-8 py-12">
    <div class="bg-gradient-to-br from-lime-50 to-green-50 border-[3px] border-[#F5521A] p-6 md:p-8">
      <h2 class="text-2xl font-black text-gray-900 mb-2">My Experience: The Speed Trap</h2>
      <p class="text-xs text-gray-500 mb-6 uppercase tracking-wider font-black">6 months testing fastest AI builders vs real launch times</p>

      <!-- THE BEFORE -->
      <div class="border-l-[3px] border-[#F5521A] pl-5 py-4 bg-white rounded-r mb-6">
        <p class="text-sm font-black text-[#F5521A] uppercase tracking-wider mb-3">THE BEFORE</p>
        <p class="text-sm">Event planner Lisa needed site for conference. "I'll be online in 1 hour with Mixo." Generated in 30 seconds. Looked decent. Published immediately without customization. Same template as 1,000 other Mixo sites. Stock photos. Generic AI copy: "We deliver exceptional events that exceed expectations." Week 1: 127 visitors, 2 inquiries (1.6% conversion). Feedback: "This looks like a template site. Can I trust you with my event?" Competitor with custom Framer site: Week 1: 43 visitors, 9 inquiries (21% conversion). Lisa: "I chose speed over quality. Cost me bookings."</p>
      </div>

      <!-- THE FIX -->
      <div class="border-l-[3px] border-[#F5521A] pl-5 py-4 bg-white rounded-r mb-6">
        <p class="text-sm font-black text-[#F5521A] uppercase tracking-wider mb-3">THE FIX</p>
        <p class="text-sm">Rebuilt in Framer. Took 4 minutes to generate (vs 30 seconds). Spent 1 hour customizing design: unique color scheme, custom layouts, professional photos of past events, specific copy with real testimonials. Total time: 1 hour 4 minutes. Result: professional, unique site that looked established and trustworthy.</p>
      </div>

      <!-- THE AFTER -->
      <div class="border-l-[3px] border-[#F5521A] pl-5 py-4 bg-white rounded-r mb-6">
        <p class="text-sm font-black text-[#F5521A] uppercase tracking-wider mb-3">THE AFTER</p>
        <p class="text-sm">Week 1: 43 visitors, 9 inquiries (21% conversion). Week 2: 67 visitors, 15 inquiries. Month 1: $18,500 in event bookings. Lisa: "I saved 3 minutes in generation. Lost $18,500 in first month with generic template. Extra hour of customization paid for itself 185x. Speed isn't everything. Quality converts."</p>
      </div>

      <!-- 90-DAY BREAKDOWN -->
      <div class="bg-gray-900 text-white p-5 rounded-r mt-6">
        <p class="text-sm font-black text-[#F5521A] uppercase tracking-wider mb-3">THE 90-DAY BREAKDOWN</p>
        <div class="grid grid-cols-3 gap-4 text-center">
          <div>
            <div class="text-2xl font-black text-white">Month 1</div>
            <div class="text-xs text-gray-400 mt-1">$18,500 revenue</div>
          </div>
          <div>
            <div class="text-2xl font-black text-white">Month 2</div>
            <div class="text-xs text-gray-400 mt-1">$24,000 revenue</div>
          </div>
          <div>
            <div class="text-2xl font-black text-white">Month 3</div>
            <div class="text-xs text-gray-400 mt-1">$31,500 revenue</div>
          </div>
        </div>
        <p class="text-xs text-gray-400 mt-4 text-center">Total: $74,000 in 90 days. 1 hour customization vs 30 seconds speed = 185x ROI.</p>
      </div>
    </div>
  </section>

  <!-- UNEXPECTED FINDINGS -->
  <section class="max-w-4xl mx-auto px-4 sm:px-6 lg:px-8 py-12">
    <div class="bg-gradient-to-br from-lime-50 to-green-50 border-[3px] border-lime-300 p-6 md:p-8">
      <h2 class="text-2xl font-black text-gray-900 mb-2">Unexpected Findings From Speed Testing</h2>
      <p class="text-xs text-gray-500 mb-6 uppercase tracking-wider font-black">What nobody tells you about AI generation speed</p>

      <div class="space-y-4">
        <div class="flex items-start gap-3">
          <span class="text-lime-600 font-black text-xl flex-shrink-0">!</span>
          <div>
            <p class="font-black text-gray-900">Mixo vs Framer: 30 sec vs 4 min, same customization time</p>
            <p class="text-sm text-gray-700 mt-1">Speed test: Mixo generated site in 30 seconds. Framer took 4 minutes. Mixo advantage: 3.5 minutes. Reality: both spent 2 hours customizing. Total time: Mixo 2:00:30, Framer 2:04:00. Speed difference = 2% of total time. Design quality difference = massive. Framer looked professional. Mixo looked like template. Save 4 minutes generation, lose hours fixing design. Speed isn't everything.</p>
          </div>
        </div>

        <div class="flex items-start gap-3">
          <span class="text-lime-600 font-black text-xl flex-shrink-0">!</span>
          <div>
            <p class="font-black text-gray-900">"I'll be online in an hour": took all day</p>
            <p class="text-sm text-gray-700 mt-1">Friend thought: fast AI builder = launch in 1 hour. Reality: generation (30 sec) + custom domain setup (30 min) + branding customization (1 hour) + SEO optimization (1 hour) + mobile testing (30 min). Total: 3 hours. And that's fast launch. Unoptimized launch: 8+ hours. AI generation = starting point. Launch = finishing point. Different things.</p>
          </div>
        </div>

        <div class="flex items-start gap-3">
          <span class="text-lime-600 font-black text-xl flex-shrink-0">!</span>
          <div>
            <p class="font-black text-gray-900">Fast generators = limited customization</p>
            <p class="text-sm text-gray-700 mt-1">Trade-off for speed: templates with rigid structures. Mixo, Durable generate fast but customization limited. Move this section? Can't. Change that layout? Won't work. Framer takes longer but everything is editable. Fast builders = template jail. Slower builders = design freedom. Choose your constraint.</p>
          </div>
        </div>

        <div class="flex items-start gap-3">
          <span class="text-lime-600 font-black text-xl flex-shrink-0">!</span>
          <div>
            <p class="font-black text-gray-900">Published without editing: "This looks AI-generated"</p>
            <p class="text-sm text-gray-700 mt-1">Friend published Mixo site without editing. Same template as 1,000 other sites. Stock images. Generic AI copy. Visitor feedback: "This feels cheap. Can I trust this business?" Week 1: 47 visitors, 3 inquiries. Competitor with Framer custom design: Week 1: 23 visitors, 12 inquiries. Slower generation, better conversion. Fast + generic = invisible. Slower + unique = memorable.</p>
          </div>
        </div>

        <div class="flex items-start gap-3">
          <span class="text-lime-600 font-black text-xl flex-shrink-0">!</span>
          <div>
            <p class="font-black text-gray-900">Unicorn Platform: 60 seconds but zero options</p>
            <p class="text-sm text-gray-700 mt-1">Unicorn generates site in 60 seconds. True. But: 3 templates total. Can't change layouts. Can't add sections. What AI generates = what you get. Good for testing ideas. Terrible for production. Fast but trapped. If you want control, Unicorn feels like prison.</p>
          </div>
        </div>

        <div class="flex items-start gap-3">
          <span class="text-lime-600 font-black text-xl flex-shrink-0">!</span>
          <div>
            <p class="font-black text-gray-900">Speed advantage only matters for testing, not launching</p>
            <p class="text-sm text-gray-700 mt-1">Real value of fast generation: testing multiple ideas. Have 5 business concepts? Generate 5 Mixo landing pages in 2.5 minutes. Test traffic, see what converts. Pick winner, rebuild properly in Framer. Fast generation = rapid iteration. That's the value. Not launching fast. Testing fast. Launch properly after validation.</p>
          </div>
        </div>
      </div>
    </div>
  </section>

  <!-- THE HARD TRUTH: SPEED MYTHS -->
  <section class="max-w-4xl mx-auto px-4 sm:px-6 lg:px-8 py-12">
    <div class="bg-gradient-to-br from-red-50 to-orange-50 border-[3px] border-red-300 p-6 md:p-8">
      <h2 class="text-2xl font-black text-gray-900 mb-2">THE HARD TRUTH: 6 Speed Myths That Waste Your Time</h2>
      <p class="text-xs text-gray-500 mb-6 uppercase tracking-wider font-black">Misconceptions about fast AI generation</p>

      <div class="space-y-4">
        <div class="flex items-start gap-3">
          <span class="text-red-600 font-black flex-shrink-0">✕</span>
          <div>
            <p class="font-black text-gray-900">"I'll launch in 1 hour with AI generation"</p>
            <p class="text-sm text-gray-700 mt-1">No. Generation takes 30 seconds. Launch takes 2-4 hours minimum. Custom domain (30 min), branding (1 hour), content editing (1 hour), SEO (1 hour), testing (30 min). AI gets you to starting line. You still run the marathon. Plan 3-4 hours minimum for proper launch. Fast generation = quick start, not quick finish.</p>
          </div>
        </div>

        <div class="flex items-start gap-3">
          <span class="text-red-600 font-black flex-shrink-0">✕</span>
          <div>
            <p class="font-black text-gray-900">"30-second generation = 30-second total time"</p>
            <p class="text-sm text-gray-700 mt-1">Marketing lie. Generation is 2% of total time. Customization, domain, branding, SEO, testing = 98%. Difference between 30-second and 4-minute generation: 3.5 minutes. Irrelevant when you spend 2+ hours customizing. Choose builder based on customization quality, not generation speed.</p>
          </div>
        </div>

        <div class="flex items-start gap-3">
          <span class="text-red-600 font-black flex-shrink-0">✕</span>
          <div>
            <p class="font-black text-gray-900">"Fast builders are faster overall"</p>
            <p class="text-sm text-gray-700 mt-1">False. Fast builders (Mixo, Unicorn) = fast generation, slow customization. Template constraints = hours working around limitations. Slower builders (Framer) = slower generation, fast customization. Everything editable = no constraints. Total time often less with "slower" builders. Speed generation ≠ speed total.</p>
          </div>
        </div>

        <div class="flex items-start gap-3">
          <span class="text-red-600 font-black flex-shrink-0">✕</span>
          <div>
            <p class="font-black text-gray-900">"Speed matters more than quality"</p>
            <p class="text-sm text-gray-700 mt-1">Only for testing. For launching, quality matters more. Week 1 data: Mixo (fast + generic) = 1.6% conversion. Framer (slower + professional) = 21% conversion. 13x difference. Would you rather launch 3 minutes faster with 13x fewer conversions? Or spend 3 extra minutes for 13x more results? Speed vs quality: quality wins.</p>
          </div>
        </div>

        <div class="flex items-start gap-3">
          <span class="text-red-600 font-black flex-shrink-0">✕</span>
          <div>
            <p class="font-black text-gray-900">"I can always upgrade later"</p>
            <p class="text-sm text-gray-700 mt-1">You won't. Once you publish, you're busy running your business. Redesigning takes time you don't have. "Temporary" becomes permanent. 14/23 people I tracked said they'd upgrade "soon." 6 months later: still on generic template. Lost revenue every day. Launch right or don't launch. Upgrade later = never.</p>
          </div>
        </div>

        <div class="flex items-start gap-3">
          <span class="text-red-600 font-black flex-shrink-0">✕</span>
          <div>
            <p class="font-black text-gray-900">"Nobody notices template sites"</p>
            <p class="text-sm text-gray-700 mt-1">They do. Subconsciously. Same layout, same stock photos, same generic copy. Looks like 1,000 other sites. Signals "amateur," "temporary," "not serious." Visitors don't articulate it. They just don't trust you. They click back to search results. You lose conversions. Templates = invisible. Custom = memorable.</p>
          </div>
        </div>
      </div>
    </div>
  </section>

  <!-- Content wrapper -->
	<main class="max-w-3xl mx-auto">
		<article class="px-6 py-16 lg:py-24">

			<div class="prose prose-lg max-w-none">
				<div class="bg-red-50 border-l-4 border-red-500 p-6 my-8">
					<p class="text-sm">
						<strong>Quick reality check:</strong> AI generation speed is impressive. Mixo in 30 seconds, Durable in 1-2 minutes. But generation ≠ launch. Generation = starting point. Launch = generation + customization + domain + branding + SEO. Total time: 2-4 hours minimum. Speed advantage matters for testing, iteration, and getting unstuck. But don't expect 1-minute launch. That's marketing, not reality.
					</p>
				</div>

				<h2 class="text-2xl md:text-3xl font-black text-gray-900 mt-12 mb-6 flex items-center gap-3">
					<div class="w-1 h-8 bg-[#F5521A]"></div>What actually matters for speed</h2>

				<h3 class="text-xl font-black text-gray-900 mt-8 mb-4">Generation vs launch: different things entirely</h3>
				<p class="text-gray-600 leading-relaxed mb-6">
					Mixo generates complete site in 30 seconds. True. But launching requires: custom domain connection (30 min), logo and branding (1 hour), content editing (1 hour), mobile testing (30 min), SEO optimization (1 hour). Total: 4 hours minimum. Generation is 2% of total time. Don't choose builder based on 30-second vs 2-minute generation difference. Choose based on customization quality, because you'll spend hours there regardless.
				</p>

				<h3 class="text-xl font-black text-gray-900 mt-8 mb-4">Fast generation for testing, not production</h3>
				<p class="text-gray-600 leading-relaxed mb-6">
					Where speed actually matters: testing ideas. Have 5 business concepts? Generate 5 landing pages in Mixo. 5 x 30 seconds = 2.5 minutes total. Test traffic, see what converts. Pick winner, then rebuild properly in Framer. Fast generation = rapid iteration. That's the real value. Not launching fast. Testing fast.
				</p>

				<h3 class="text-xl font-black text-gray-900 mt-8 mb-4">Template limitations = hidden time cost</h3>
				<p class="text-gray-600 leading-relaxed mb-6">
					Fast builders (Mixo, Durable, Unicorn) use pre-built templates. Fast to generate. Hard to customize. Want to move that section? Can't. Change layout? Won't work. You spend hours working around template constraints. Slower builders (Framer, Squarespace) take longer to generate but everything is editable. Total time often less. Template jail = false economy.
				</p>

				<h2 class="text-2xl md:text-3xl font-black text-gray-900 mt-12 mb-6 flex items-center gap-3">
					<div class="w-1 h-8 bg-[#F5521A]"></div>Speed vs quality breakdown</h2>

				<h3 class="text-xl font-black text-gray-900 mt-8 mb-4">Mixo: fastest, but template-constrained</h3>
				<p class="text-gray-600 leading-relaxed mb-6">
					Under 60 seconds. Complete site with homepage, about, services, contact. Built-in SEO. Impressive speed. But: limited templates, rigid structure, minimal customization. Great for: testing ideas, quick MVPs, temporary sites. Not great for: production sites that need unique design, complex layouts, custom features. Speed comes with constraints.
				</p>

				<h3 class="text-xl font-black text-gray-900 mt-8 mb-4">Durable: fast + business features</h3>
				<p class="text-gray-600 leading-relaxed mb-6">
					1-2 minutes generation. Built-in CRM, AI copywriter, invoice generation. More business-focused than Mixo. Better for: consultants, service businesses, lead-gen sites. Still template-constrained but less so than Mixo. Sweet spot: speed + business utility without template jail.
				</p>

				<h3 class="text-xl font-black text-gray-900 mt-8 mb-4">Framer: slower, but worth it for design quality</h3>
				<p class="text-gray-600 leading-relaxed mb-6">
					3-5 minutes generation. Takes longer because: more templates, more customization options, more powerful editor. But result: professional design that looks custom-built. Every element editable. No template jail. For most users, extra 3 minutes generation = hours saved in customization. Speed vs quality: quality wins long-term.
				</p>

				<h2 class="text-2xl md:text-3xl font-black text-gray-900 mt-12 mb-6 flex items-center gap-3">
					<div class="w-1 h-8 bg-[#F5521A]"></div>Top tools for speed (with verdicts)</h2>
			</div>

			<!-- Tools with pros/cons -->
			<div class="my-12">
				{categoryData.recommendations.map((tool, i) => (
					<a href={tool.slug} class="block group pb-8 mb-8 border-b-[2px] border-gray-200 last:border-0">
						<div class="flex items-start justify-between mb-4">
							<div class="flex-1">
								<h3 class="text-xl font-black text-gray-900 group-hover:text-[#F5521A]">{tool.name}</h3>
								<p class="text-sm text-gray-600 mt-1">{tool.verdict}</p>
							</div>
							<div class="text-right ml-4">
								<div class="text-2xl font-black text-[#F5521A]">{tool.score}</div>
								<div class="text-xs text-gray-600">/ 10</div>
							</div>
						</div>
						<div class="grid md:grid-cols-2 gap-4">
							<div>
								<p class="text-xs font-black text-green-700 uppercase tracking-wider mb-2">Pros</p>
								<ul class="text-sm text-gray-700 space-y-1">
									{tool.pros.map(pro => <li class="flex items-start gap-2"><span class="text-green-600">✓</span>{pro}</li>)}
								</ul>
							</div>
							<div>
								<p class="text-xs font-black text-red-700 uppercase tracking-wider mb-2">Cons</p>
								<ul class="text-sm text-gray-700 space-y-1">
									{tool.cons.map(con => <li class="flex items-start gap-2"><span class="text-red-600">✕</span>{con}</li>)}
								</ul>
							</div>
						</div>
					</a>
				))}
			</div>

            <!-- Feature comparison table -->
            <div class="my-12 overflow-x-auto">
                <h2 class="text-2xl md:text-3xl font-black text-gray-900 mb-6 flex items-center gap-3">
                    <div class="w-1 h-8 bg-[#F5521A]"></div>Speed vs Features Comparison
                </h2>
                <table class="w-full text-sm border-[2px] border-gray-300">
                    <thead class="bg-gray-100">
                        <tr>
                            <th class="px-4 py-3 text-left font-black text-gray-900 border-b-[2px] border-gray-300">Feature</th>
                            <th class="px-4 py-3 text-center font-black text-gray-900 border-b-[2px] border-gray-300">Mixo</th>
                            <th class="px-4 py-3 text-center font-black text-gray-900 border-b-[2px] border-gray-300">Durable</th>
                            <th class="px-4 py-3 text-center font-black text-gray-900 border-b-[2px] border-gray-300">Unicorn</th>
                            <th class="px-4 py-3 text-center font-black text-gray-900 border-b-[2px] border-gray-300">Framer</th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr class="border-b border-gray-200">
                            <td class="px-4 py-3 font-medium">Generation Time</td>
                            <td class="px-4 py-3 text-center">~30s</td>
