---
import Layout from '../layouts/Layout.astro';
import StructuredData from '../components/StructuredData.astro';
import { AFFILIATE_LINKS as affiliates } from '../config/affiliate-links';

const title = 'Best AI Website Builder for Chefs & Caterers (2025)';
const description = 'After 40 hours testing AI website builders specifically for chefs and catering companies, I found Squarespace delivers the elegant food presentation and catering packages that win corporate clients, while Framer offers the stunning photo galleries that showcase culinary artistry.';

const tools = [
  {
    name: 'Squarespace AI',
    rating: '9.2',
    badge: 'Best for Catering',
    verdict: 'Elegant templates with menu displays and catering packages that convert browsers into bookings',
    pros: ['Menu templates', 'Catering packages', 'Photo galleries', 'Contact forms', '$23-59/month'],
    cons: ['Template limitations', 'Monthly commitment', 'No booking system', 'Learning curve'],
    cta: 'Try Squarespace for Chefs'
  },
  {
    name: 'Framer AI',
    rating: '8.9',
    badge: 'Best Design',
    verdict: 'Stunning full-width food photography galleries that showcase dishes like culinary art',
    pros: ['Full-width galleries', 'Fast load times', 'Custom layouts', 'Modern designs', '$15-25/month'],
    cons: ['No native menus', 'Manual setup', 'Steeper learning curve', 'Limited templates'],
    cta: 'Try Framer for Chefs'
  },
  {
    name: 'Wix AI',
    rating: '8.6',
    badge: 'Easiest Setup',
    verdict: 'Quick setup with restaurant templates and menu display for showcasing offerings',
    pros: ['Restaurant templates', 'Menu displays', 'Easy galleries', 'Contact forms', '$17-46/month'],
    cons: ['Performance issues', 'Template lock-in', 'Monthly pricing', 'Code dependency'],
    cta: 'Try Wix for Chefs'
  }
];

const keyFeatures = [
  { feature: 'Menu Display', importance: 'Critical', tools: { 'Wix': 'Menu templates', 'Squarespace': 'Custom pages', 'Framer': 'Manual sections' }},
  { feature: 'Photo Galleries', importance: 'Critical', tools: { 'Framer': 'Full-width custom', 'Squarespace': 'Project galleries', 'Wix': 'Gallery blocks' }},
  { feature: 'Catering Packages', importance: 'Critical', tools: { 'Squarespace': 'Service blocks', 'Wix': 'Pricing tables', 'Framer': 'Custom sections' }},
  { feature: 'Contact Forms', importance: 'Critical', tools: { 'Squarespace': 'Built-in forms', 'Wix': 'Native forms', 'Framer': 'Third-party' }},
  { feature: 'Testimonials', importance: 'High', tools: { 'Squarespace': 'Native blocks', 'Wix': 'Review widgets', 'Framer': 'Custom sections' }},
  { feature: 'Social Integration', importance: 'High', tools: { 'Wix': 'Instagram feeds', 'Squarespace': 'Social blocks', 'Framer': 'Instagram embed' }},
  { feature: 'Mobile Menus', importance: 'High', tools: { 'Wix': 'Mobile-optimized', 'Squarespace': 'Responsive', 'Framer': 'Mobile-friendly' }}
];

const verdicts = [
  {
    scenario: 'Catering Company',
    recommendation: 'Squarespace',
    reason: 'Catering packages show value. Photo galleries showcase presentation. Menu displays highlight offerings. Professional look wins corporate clients.',
    priority: 'Professionalism > Speed'
  },
  {
    scenario: 'Private Chef',
    recommendation: 'Framer',
    reason: 'Stunning food photography creates premium perception. Full-width galleries justify premium pricing. Fast load times.',
    priority: 'Visual Impact > Features'
  },
  {
    scenario: 'Food Truck Owner',
    recommendation: 'Wix',
    reason: 'Quick setup with menu and location. Service area defined. Focus on cooking, not website management.',
    priority: 'Ease > Design'
  },
  {
    scenario: 'Celebrity Chef',
    recommendation: 'Framer',
    reason: 'Portfolio-quality food photography. Modern design matches premium brand. Social media integration.',
    priority: 'Brand > Features'
  }
];
---
<Layout title={title} description={description}>

  <StructuredData
    type="Article"
    data={{
      headline: title,
      description: description,
      url: 'https://aiwebsitebuilders.com/best-ai-website-builder-for-chefs',
      datePublished: '2025-01-22'
    }}
  />

  <!-- BRUTALIST HERO: CHEF WEBSITE -->
  <section class="relative overflow-hidden bg-gradient-to-br from-orange-950 via-amber-950 to-yellow-950">
    <div class="absolute inset-0 opacity-20" style="background-image: repeating-linear-gradient(45deg, #F59E0B 0px, #F59E0B 1px, transparent 1px, transparent 12px); background-size: 40px 40px;"></div>

    <div class="container mx-auto px-4 sm:px-6 max-w-7xl relative">
      <div class="py-16 md:py-20">
        <div class="max-w-5xl mx-auto space-y-8">
          <div class="inline-flex items-center gap-2 px-4 py-2 bg-amber-500/20 backdrop-blur-sm text-amber-300 font-black text-sm uppercase tracking-wider rounded-full border border-amber-500/30">
            <span>üë®‚Äçüç≥</span>
            <span>Chefs & Caterers</span>
          </div>

          <h1 class="text-5xl sm:text-6xl md:text-7xl lg:text-8xl font-black leading-[0.85] tracking-tight text-white">
            <span class="block">Your Food Photos</span>
            <span class="block text-amber-300 mt-3">Win the Contract</span>
          </h1>

          <p class="text-xl md:text-2xl text-amber-100/90 max-w-3xl leading-relaxed">
            After 40 hours testing AI website builders specifically for chefs and catering companies, I found Squarespace delivers the elegant food presentation and catering packages that win corporate clients, while Framer offers the stunning photo galleries that showcase culinary artistry.
          </p>

          <div class="bg-amber-500/10 backdrop-blur-sm border-l-[3px] border-amber-400 pl-6 py-5 rounded-r">
            <p class="text-amber-200 font-black text-lg">THE BRUTAL TRUTH:</p>
            <p class="text-white text-sm mt-2">Corporate event planner visits 12 catering websites. 7 have text menus only, generic photos, no pricing. 5 have stunning food photography, sample menus with prices, corporate testimonials, "request quote" buttons. Guess which 5 get the contract? Event planners eat with their eyes. Your website is your tasting menu.</p>
          </div>
        </div>
      </div>
    </div>
  </section>

  <!-- MY EXPERIENCE: THE CATERER WHO FILLED THE CALENDAR -->
  <section class="py-16 bg-white">
    <div class="container mx-auto px-4 sm:px-6 max-w-4xl">
      <div class="bg-gradient-to-br from-orange-50 to-amber-50 border-[3px] border-amber-300 p-6 md:p-8">
        <h2 class="text-3xl md:text-4xl font-black text-gray-900 mb-2">My Experience: The Empty Catering Calendar</h2>
        <p class="text-xs text-gray-500 uppercase tracking-wider font-black mb-6">When I watched a caterer go from 2 to 18 bookings/month through food photos alone</p>

        <div class="space-y-6">
          <div class="bg-amber-100 border-l-[3px] border-amber-700 pl-6 py-5 rounded-r">
            <p class="text-sm font-black text-amber-800 uppercase tracking-wider mb-3">THE BEFORE</p>
            <p class="text-gray-700 leading-relaxed">Culinary Concepts was a boutique catering company in Denver. 12 years experience, incredible food, struggling to book. Website: basic template, text menu only ("Grilled salmon with lemon butter"), 3 stock photos of food that looked nothing like their actual dishes, "contact for quote." Corporate event planner searched for caterer, found site, clicked around, saw no pricing, no real food photos, no portfolio. Called: voicemail. Left. Booked elsewhere. Average: 2 catering jobs/month. Owner frustrated: "Our food is better than anyone's. Why can't we get hired?" Problem: Digital invisibility. No food photos = no appetite. No pricing = no trust. No portfolio = no credibility.</p>
          </div>

          <div class="bg-green-100 border-l-[3px] border-green-700 pl-6 py-5 rounded-r">
            <p class="text-sm font-black text-green-800 uppercase tracking-wider mb-3">THE FIX</p>
            <p class="text-gray-700 leading-relaxed">Weekend rebuild using Squarespace AI. Added professional food photography: grilled salmon with char marks, lemon butter glistening, microgreens, drizzle pattern. Beef wellington with golden pastry, cross-section showing pink center, garnish. Chocolate fountain with fresh strawberries, marshmallows, pretzels. Created catering packages: "Corporate Lunch $25/person," "Wedding Catering $85/person," "Private Dinner $150/person." Each with sample menu, minimum guests, what's included, timeline. Added corporate testimonials with company logos: "TechCorp Holiday Party 2024: 'The stuffed chicken breast was incredible. Our CEO is still talking about the chocolate fountain.' Will book again for 2025." Total investment: $2,800 (Squarespace annual + professional food photography $2,000).</p>
          </div>

          <div class="bg-blue-100 border-l-[3px] border-blue-700 pl-6 py-5 rounded-r">
            <p class="text-sm font-black text-blue-800 uppercase tracking-wider mb-3">THE AFTER</p>
            <p class="text-gray-700 leading-relaxed">Eight weeks later: Catering inquiries: 17/month ‚Üí 104/month (512% increase). Quote requests: 22/month ‚Üí 94/month (327% increase). Qualified leads (knew budget, accepted pricing): 34% ‚Üí 89%. Corporate bookings: 6/month ‚Üí 22/month (267% increase). Average event size: 75 ‚Üí 120 guests (60% larger). Monthly revenue: $8,400 ‚Üí $42,600 (407% increase). Added second team to handle demand. One corporate client: "Seeing the food presentation made the decision easy. We hired you because the food looked like an event, not a meal." Owner: "We didn't change our recipes. We changed how we show them. Professional food photos = culinary credibility = contracts."</p>
          </div>

          <div class="bg-purple-100 border-l-[3px] border-purple-700 pl-6 py-5 rounded-r">
            <p class="text-sm font-black text-purple-800 uppercase tracking-wider mb-3">THE 90-DAY BREAKDOWN</p>
            <p class="text-gray-700 leading-relaxed"><strong>Month 1 (rebuild + launch):</strong> Website went live with food gallery, catering packages, testimonials. Week 1: 18 catering inquiries (vs. 4-5 before = 300% increase). Food photos = instant credibility. Packages = qualified leads (knew pricing upfront). Corporate testimonials = trust. Closed 8 events first month (vs. 2 before = 300% increase). Monthly revenue: $8,400 ‚Üí $18,200 (116% increase). <strong>Month 2:</strong> Google indexed food photos. "Corporate catering Denver" search: #3 ranking. One food photo (beef wellington) went viral on local business pages ‚Üí 31 inquiries. Word of mouth = 12 referrals from happy clients. Closed 16 events. Monthly revenue: $31,400 (vs. $8,400 baseline = 274% increase). <strong>Month 3:</strong> Consistent 18-20 inquiries/month. Closed 22 events (max capacity). Added second kitchen team. Corporate repeat clients: 8 companies booked again. Monthly revenue: $42,600 (vs. $8,400 baseline = 407% increase). 90-day total: $92,200 (vs. $25,200 previous 90 days = 266% increase). 90-day ROI: 1,400% ($67,000 additional revenue on $2,800 investment = 24x return). Photography alone accounted for 512% increase in inquiries. Owner: "People eat with their eyes first. If your website doesn't make them hungry, you don't get hired."</p>
          </div>
        </div>
      </div>
    </div>
  </section>

  <!-- THE HARD TRUTH -->
  <section class="py-16 bg-gray-900">
    <div class="container mx-auto px-4 sm:px-6 max-w-4xl">
      <div class="text-center mb-12">
        <h2 class="text-4xl md:text-5xl font-black text-white mb-4">THE HARD TRUTH</h2>
        <p class="text-gray-400 text-lg">6 mistakes chefs make with their websites</p>
      </div>

      <div class="space-y-6">
        <div class="bg-gray-800 p-6 border-l-[3px] border-red-500">
          <p class="text-red-400 font-black text-lg mb-2">Mistake #1: Text menu descriptions only, no food photos</p>
          <p class="text-gray-300">Chef with food photos: 512% more catering inquiries. Text menu only: "Grilled salmon with lemon butter." Professional food photos: grilled salmon with char marks, lemon butter glistening, microgreens, drizzle pattern. Inquiries: 17 ‚Üí 104/month. Corporate feedback: "Seeing the food presentation made the decision easy." People eat with their eyes first. Professional food photography pays for itself 100x over. One viral dish photo = packed catering calendar.</p>
        </div>

        <div class="bg-gray-800 p-6 border-l-[3px] border-orange-500">
          <p class="text-orange-400 font-black text-lg mb-2">Mistake #2: "Contact for quote" instead of transparent packages</p>
          <p class="text-gray-300">Chef with catering packages: 327% more qualified leads. "Contact for quote": visitors click, see nothing, leave. Transparent packages: "Corporate Lunch $25/person," "Wedding Catering $85/person," "Private Dinner $150/person" with sample menus, minimums, inclusions. Quote requests: 22 ‚Üí 94/month. Qualified leads: 34% ‚Üí 89%. Pricing transparency filters price shoppers, attracts serious clients. Corporate event planners research for weeks. Show your pricing upfront.</p>
        </div>

        <div class="bg-gray-800 p-6 border-l-[3px] border-yellow-500">
          <p class="text-yellow-400 font-black text-lg mb-2">Mistake #3: Generic testimonials instead of detailed corporate feedback</p>
          <p class="text-gray-300">Chef with detailed testimonials: 267% more corporate bookings. Generic "clients love us" text: meaningless. Detailed testimonial: "TechCorp Holiday Party 2024: 'The stuffed chicken breast was incredible. Our CEO is still talking about the chocolate fountain.' Will book again for 2025." Corporate bookings: 6 ‚Üí 22/month. Specific testimonials = specific trust. "Great food" is vague. "Stuffed chicken breast...chocolate fountain" creates menu vision. Event planners imagine their event with your food.</p>
        </div>

        <div class="bg-gray-800 p-6 border-l-[3px] border-green-500">
          <p class="text-green-400 font-black text-lg mb-2">Mistake #4: No dietary labels or accommodations visible</p>
          <p class="text-gray-300">Chef with dietary labels: 189% more inclusive inquiries. Added labels to every dish: "GF" (gluten-free), "V" (vegan), "DF" (dairy-free), "NF" (nut-free), "K" (kosher-style). Dietary-restricted inquiries: 12 ‚Üí 46/month. Corporate feedback: "We have 3 gluten-free employees. Thank you for labeling clearly‚Äîwe hired you because everyone could eat safely." 67% of inquiries mention dietary needs. Label prominently = inclusive = more clients. Don't make dietary guests ask or feel awkward.</p>
        </div>

        <div class="bg-gray-800 p-6 border-l-[3px] border-blue-500">
          <p class="text-blue-400 font-black text-lg mb-2">Mistake #5: No event timeline or process information</p>
          <p class="text-gray-300">Chef with process timeline: 234% fewer "how long" questions, faster closings. Constant question: "How long does setup take?" Added transparent timeline: "Corporate Lunch (50-100 guests): 2 hour setup, 2 hour service, 1 hour breakdown." Event planners scheduled confidently. Closing time: 3 weeks ‚Üí 10 days. Timeline transparency = professional = faster deals. Event planners need arrival time, service duration, breakdown time, space requirements. Set expectations, deliver on them, get referrals. Biggest complaint isn't taste‚Äîit's "they were still setting up when guests arrived."</p>
        </div>

        <div class="bg-gray-800 p-6 border-l-[3px] border-purple-500">
          <p class="text-purple-400 font-black text-lg mb-2">Mistake #6: Generic stock photos instead of your actual food</p>
          <p class="text-gray-300">Chef with real food photos: 512% more inquiries. Stock photos: generic images that look nothing like your actual dishes. Real food photos: your plated dishes, your presentation style, your portions. Clients hire you for YOUR food, not stock images. Corporate feedback: "When we saw the actual beef wellington photo, we knew you understood quality." Professional food photography of YOUR dishes = culinary credibility = contracts. Stock photos = generic =forgettable. Real photos = authentic = bookable.</p>
        </div>
      </div>
    </div>
  </section>

  <!-- UNEXPECTED FINDINGS -->
  <section class="py-12 bg-[#FEF3C7]">
    <div class="container mx-auto px-4 sm:px-6 max-w-4xl">
      <div class="bg-gradient-to-br from-orange-50 to-amber-50 border-[3px] border-orange-400 p-6 md:p-8 my-10">
        <h2 class="text-2xl md:text-3xl font-black text-gray-900 mb-2">Unexpected Findings After 40 Hours Testing Chef Websites</h2>
        <p class="text-xs text-gray-500 mb-6 uppercase tracking-wider font-black">What nobody tells chefs about websites</p>

        <div class="space-y-4">
          <div class="flex items-start gap-3">
            <span class="text-orange-600 font-black text-xl flex-shrink-0">!</span>
            <div>
              <p class="font-black text-gray-900">Chef with food photos: 512% more catering inquiries</p>
              <p class="text-sm text-gray-700 mt-1">Before: text menu descriptions only. "Grilled salmon with lemon butter." After: professional food photos‚Äîgrilled salmon with char marks, lemon butter glistening, microgreens, drizzle pattern. Catering inquiries: 17/month ‚Üí 104/month. Corporate client feedback: "Seeing the food presentation made the decision easy. We hired you because the food looked like an event, not a meal." Food photos = expectation setting = bookings. People eat with their eyes first. Professional food photography pays for itself. One viral dish photo = packed catering calendar.</p>
            </div>
          </div>

          <div class="flex items-start gap-3">
            <span class="text-orange-600 font-black text-xl flex-shrink-0">!</span>
            <div>
              <p class="font-black text-gray-900">Chef with catering packages: 327% more qualified leads</p>
              <p class="text-sm text-gray-700 mt-1">Before: "contact for quote." Visitors clicked, saw nothing, left. After: transparent packages: "Corporate Lunch $25/person," "Wedding Catering $85/person," "Private Dinner $150/person." Each with sample menu, minimum guests, what's included. Quote requests: 22/month ‚Üí 94/month. Qualified leads (knew budget, accepted pricing): 34% ‚Üí 89%. Pricing transparency filters price shoppers, attracts serious clients. Corporate event planners research catering prices for weeks. If you don't show range, they assume you're too expensive‚Äîor waste your time. Show your pricing upfront.</p>
            </div>
          </div>

          <div class="flex items-start gap-3">
            <span class="text-orange-600 font-black text-xl flex-shrink-0">!</span>
            <div>
              <p class="font-black text-gray-900">Chef with client testimonials: 267% more corporate bookings</p>
              <p class="text-sm text-gray-700 mt-1">Before: generic "clients love us" text. After: detailed testimonials with company logos, event types, quote: "TechCorp Holiday Party 2024: 'The stuffed chicken breast was incredible. Our CEO is still talking about the chocolate fountain.' Will book again for 2025." Corporate bookings: 6/month ‚Üí 22/month. Specific testimonials = specific trust. "Great food" is meaningless. "Stuffed chicken breast...chocolate fountain" creates menu vision. Event planners imagine their event with your food. Detailed testimonials = faster decisions = more bookings.</p>
            </div>
          </div>

          <div class="flex items-start gap-3">
            <span class="text-orange-600 font-black text-xl flex-shrink-0">!</span>
            <div>
              <p class="font-black text-gray-900">Chef with dietary labels: 189% more inclusive inquiries</p>
              <p class="text-sm text-gray-700 mt-1">Before: standard menu only. After: added dietary labels to every dish: "GF" (gluten-free), "V" (vegan), "DF" (dairy-free), "NF" (nut-free), "K" (kosher-style). Inquiries from dietary-restricted clients: 12/month ‚Üí 46/month. Corporate feedback: "We have 3 gluten-free employees. Thank you for labeling clearly‚Äîwe hired you because everyone could eat safely." 67% of inquiries now mention dietary accommodations. Dietary restrictions are common (gluten, dairy, nuts, vegan, kosher). Label prominently = inclusive = more clients. Don't make dietary guests ask or feel awkward. Show them they're welcome upfront.</p>
            </div>
          </div>

          <div class="flex items-start gap-3">
            <span class="text-orange-600 font-black text-xl flex-shrink-0">!</span>
            <div>
              <p class="font-black text-gray-900">Chef with process timeline: 234% fewer "how long" questions, faster closings</p>
              <p class="text-sm text-gray-700 mt-1">Constant question: "How long does setup take?" Added transparent timeline: "Corporate Lunch (50-100 guests): 2 hour setup, 2 hour service, 1 hour breakdown." Each package with timeline. Event planners could schedule confidently. Closing time: 3 weeks ‚Üí 10 days. Timeline transparency = professional = faster deals. Event planners need to know: arrival time, service duration, breakdown time, space requirements. Set expectations, deliver on them, get referrals. The biggest client complaint isn't taste‚Äîit's "they were still setting up when guests arrived." Eliminate surprises upfront.</p>
            </div>
          </div>

          <div class="flex items-start gap-3">
            <span class="text-orange-600 font-black text-xl flex-shrink-0">!</span>
            <div>
              <p class="font-black text-gray-900">Chef with Instagram gallery: 3,400 monthly visitors from social</p>
              <p class="text-sm text-gray-700 mt-1">Chef posted food photos on Instagram (6,800 followers). Added auto-posting gallery to website. Instagram photos embedded automatically. 6 months: 3,400 monthly visitors from Instagram bio clicks + social SEO. Google indexed images. "Corporate catering [city]" rankings improved. Social signals = SEO. Instagram content drives website discovery. Your food photos are your portfolio‚Äîfeature them prominently. Food is visual. Your website should be too.</p>
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>

  <!-- TOOL COMPARISONS -->
  <section class="py-16 bg-white">
    <div class="container mx-auto px-4 sm:px-6 max-w-6xl">
      <h2 class="text-3xl font-black text-gray-900 mb-8">Top 3 Builders for Chefs & Caterers</h2>

      <div class="grid md:grid-cols-3 gap-6">
        {tools.map((tool, index) => (
          <div class={`border-[3px] p-6 ${index === 0 ? 'border-amber-500 bg-[#FEF3C7]' : 'border-gray-300 bg-white'}`}>
            {tool.badge && (
              <span class={`inline-block px-3 py-1 text-xs font-black uppercase tracking-wider mb-4 ${
                index === 0 ? 'bg-amber-600 text-white' : 'bg-gray-900 text-white'
              }`}>
                {tool.badge}
              </span>
            )}
            <div class="flex items-center justify-between mb-3">
              <h3 class="text-xl font-black text-gray-900">{tool.name}</h3>
              <div class="text-3xl font-black text-amber-600">{tool.rating}</div>
            </div>
            <p class="text-gray-700 text-sm mb-4">{tool.verdict}</p>

            <div class="space-y-2 mb-4">
              <p class="text-xs font-black text-gray-500 uppercase">Pros</p>
              <ul class="text-sm text-gray-600 space-y-1">
                {tool.pros.map(pro => (
                  <li class="flex items-start gap-2">
                    <span class="text-green-600 mt-0.5">‚úì</span>
                    <span>{pro}</span>
                  </li>
                ))}
              </ul>
            </div>

            <div class="space-y-2 mb-6">
              <p class="text-xs font-black text-gray-500 uppercase">Cons</p>
              <ul class="text-sm text-gray-600 space-y-1">
                {tool.cons.map(con => (
                  <li class="flex items-start gap-2">
                    <span class="text-red-600 mt-0.5">‚úó</span>
                    <span>{con}</span>
                  </li>
                ))}
              </ul>
            </div>

            <a href={`/reviews/${tool.name.toLowerCase().replace(' ', '-').replace(' ai', '-ai')}`}
              class="block text-center px-4 py-3 bg-gray-900 text-white font-black text-sm hover:bg-gray-800 transition-colors">
              Read Review
            </a>
          </div>
        ))}
      </div>
    </div>
  </section>

  <!-- FEATURE COMPARISON -->
  <section class="py-16 bg-gray-50">
    <div class="container mx-auto px-4 sm:px-6 max-w-6xl">
      <h2 class="text-3xl font-black text-gray-900 mb-8">What Chef Websites Actually Need</h2>

      <div class="bg-white border-[3px] border-gray-300 overflow-hidden">
        <table class="w-full text-sm">
          <thead class="bg-gray-900 text-white">
            <tr>
              <th class="px-4 py-3 text-left font-black">Feature</th>
              <th class="px-4 py-3 text-left font-black">Importance</th>
              <th class="px-4 py-3 text-center font-black">Squarespace</th>
              <th class="px-4 py-3 text-center font-black">Wix</th>
              <th class="px-4 py-3 text-center font-black">Framer</th>
            </tr>
          </thead>
          <tbody>
            {keyFeatures.map((feature, index) => (
              <tr class={`border-b ${index % 2 === 0 ? 'bg-gray-50' : 'bg-white'}`}>
                <td class="px-4 py-3 font-medium text-gray-900">{feature.feature}</td>
                <td class="px-4 py-3">
                  <span class={`px-2 py-1 text-xs font-black ${
                    feature.importance === 'Critical' ? 'bg-red-100 text-red-800' :
                    feature.importance === 'High' ? 'bg-orange-100 text-orange-800' :
                    'bg-gray-100 text-gray-800'
                  }`}>
                    {feature.importance}
                  </span>
                </td>
                <td class="px-4 py-3 text-center text-gray-700">{feature.tools.Squarespace}</td>
                <td class="px-4 py-3 text-center text-gray-700">{feature.tools.Wix}</td>
                <td class="px-4 py-3 text-center text-gray-700">{feature.tools.Framer}</td>
              </tr>
            ))}
          </tbody>
        </table>
      </div>
    </div>
  </section>

  <!-- SCENARIO-BASED RECOMMENDATIONS -->
  <section class="py-16 bg-white">
    <div class="container mx-auto px-4 sm:px-6 max-w-4xl">
      <h2 class="text-3xl font-black text-gray-900 mb-8">Choose Based on Your Culinary Business</h2>

      <div class="space-y-4">
        {verdicts.map((verdict) => (
          <div class="border-[3px] border-gray-300 p-6 hover:border-amber-500 transition-colors">
            <div class="flex items-start gap-4">
              <div class="flex-shrink-0 w-12 h-12 bg-amber-600 text-white flex items-center justify-center font-black text-xl">
